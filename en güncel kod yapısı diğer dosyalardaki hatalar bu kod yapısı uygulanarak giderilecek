cution_count":29,"outputs":[]},{"cell_type":"code","source":["class DistilBertClassifier(nn.Module):\n","    def __init__(self, base, hidden=768, n_classes=3, dropout=0.2):\n","        super().__init__()\n","        self.base = base\n","        self.drop = nn.Dropout(dropout)\n","        self.fc = nn.Linear(hidden, n_classes)\n","\n","    def forward(self, input_ids, attention_mask):\n","        out = self.base(input_ids=input_ids, attention_mask=attention_mask)\n","        # DistilBERT: out.last_hidden_state [B, T, H]\n","        cls = out.last_hidden_state[:, 0, :]   # [CLS]-like first token\n","        x = self.drop(cls)\n","        return self.fc(x)\n"],"metadata":{"id":"Ve87bZLeB1aE","executionInfo":{"status":"ok","timestamp":1768503598871,"user_tz":-180,"elapsed":19,"user":{"displayName":"Hakan Aytuğ Fırat","userId":"02958237446288372335"}}},"execution_count":30,"outputs":[]},{"cell_type":"code","source":["from sklearn.metrics import accuracy_score, f1_score\n","def compute_metrics(y_true, y_pred):\n","    return accuracy_score(y_true, y_pred), f1_score(y_true, y_pred, average=\"macro\")\n","\n","EPOCHS = 3\n","BATCH = 32\n","LR = 2e-5\n","WD = 0.01\n","\n","distil_accs, distil_f1s = [], []\n","\n","for fold_idx, (tr_idx, te_idx) in enumerate(folds, 1):\n","    print(f\"\\n===== TEXT_DISTILBERT | Fold {fold_idx} =====\")\n","    tr_texts = df[\"text\"].iloc[tr_idx].tolist()\n","    te_texts = df[\"text\"].iloc[te_idx].tolist()\n","    ytr = df[\"y\"].iloc[tr_idx].values\n","    yte = df[\"y\"].iloc[te_idx].values\n","\n","    train_loader = DataLoader(TextTfDataset(tr_texts, ytr), batch_size=BATCH, shuffle=True)\n","    test_loader  = DataLoader(TextTfDataset(te_texts, yte), batch_size=BATCH, shuffle=False)\n","\n","    model = DistilBertClassifier(AutoModel.from_pretrained(MODEL_NAME)).to(device)\n","\n","    opt = torch.optim.AdamW(model.parameters(), lr=LR, weight_decay=WD)\n","    total_steps = EPOCHS * len(train_loader)\n","    sched = get_linear_schedule_with_warmup(opt, num_warmup_steps=int(0.1*total_steps), num_training_steps=total_steps)\n","\n","    loss_fn = nn.CrossEntropyLoss()\n","    scaler = torch.cuda.amp.GradScaler(enabled=(device==\"cuda\"))\n","\n","    best_f1, patience, PATIENCE = -1, 0, 2\n","\n","    for ep in range(1, EPOCHS+1):\n","        model.train()\n","        total_loss = 0.0\n","\n","        for batch in tqdm(train_loader, desc=f\"Fold{fold_idx} ep{ep}\", leave=False):\n","            input_ids = batch[\"input_ids\"].to(device)\n","            attention_mask = batch[\"attention_mask\"].to(device)\n","            yb = batch[\"y\"].to(device)\n","\n","            opt.zero_grad(set_to_none=True)\n","            with torch.cuda.amp.autocast(enabled=(device==\"cuda\")):\n","                logits = model(input_ids, attention_mask)\n","                loss = loss_fn(logits, yb)\n","\n","            scaler.scale(loss).backward()\n","            scaler.step(opt)\n","            scaler.update()\n","            sched.step()\n","\n","            total_loss += loss.item() * len(yb)\n","\n","        # eval\n","        model.eval()\n","        preds, trues = [], []\n","        with torch.no_grad():\n","            for batch in test_loader:\n","                input_ids = batch[\"input_ids\"].to(device)\n","                attention_mask = batch[\"attention_mask\"].to(device)\n","                logits = model(input_ids, attention_mask)\n","                p = torch.argmax(logits, dim=1).cpu().numpy()\n","                preds.extend(p.tolist())\n","                trues.extend(batch[\"y\"].numpy().tolist())\n","\n","        acc, f1m = compute_metrics(trues, preds)\n","        print(f\"[TEXT_DISTIL] ep {ep:02d}/{EPOCHS} | loss={total_loss/len(tr_idx):.4f} | acc={acc:.3f} | macroF1={f1m:.3f}\")\n","\n","        if f1m > best_f1 + 1e-4:\n","            best_f1 = f1m\n","            patience = 0\n","            best_state = {k:v.detach().cpu().clone() for k,v in model.state_dict().items()}\n","        else:\n","            patience += 1\n","            if patience >= PATIENCE:\n","                break\n","\n","    model.load_state_dict(best_state)\n","\n","    # final eval\n","    model.eval()\n","    preds, trues = [], []\n","    with torch.no_grad():\n","        for batch in test_loader:\n","            input_ids = batch[\"input_ids\"].to(device)\n","            attention_mask = batch[\"attention_mask\"].to(device)\n","            logits = model(input_ids, attention_mask)\n","            p = torch.argmax(logits, dim=1).cpu().numpy()\n","            preds.extend(p.tolist())\n","            trues.extend(batch[\"y\"].numpy().tolist())\n","\n","    acc, f1m = compute_metrics(trues, preds)\n","    distil_accs.append(acc); distil_f1s.append(f1m)\n","\n","    print(f\"[TEXT_DISTIL] Fold {fold_idx} FINAL | Acc={acc:.3f} | MacroF1={f1m:.3f}\")\n","\n","print(\"\\n=== TEXT_DISTILBERT SUMMARY ===\")\n","print(\"Acc : %.3f ± %.3f\" % (np.mean(distil_accs), np.std(distil_accs)))\n","print(\"F1m : %.3f ± %.3f\" % (np.mean(distil_f1s), np.std(distil_f1s)))\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":1000,"referenced_widgets":["92630860dcdc40f0bac39ca1d4ace78f","6abf9acee0e34eaa9f783e6fd15db530","fd774db771cc42b0bb4385c4a2b59576","7794be80e9be4f2f976cc7dafd0472af","e31e6f552ee04badb8bd1fdbfffb205a","a421be9345d6498f97f0de4d201594ec","b55969770b1042a7895e70ab053a85b6","932f8719e41f43b28d990a0434800242","5f7040438bfa4a8795f13c86554dabf0","17bafd33fd5a4b4aa47f062b245005e0","328212f1f32a4609bacd5d5694b82daf","9f8b813b2d9c46609f7e93fb0eb92e64","79b855b32b544f67acc1b7e4bbae02f0","8476fc231fa44439aed9644e7e75e808","799d00f520fc4419a99ad4e9257d0bac","f5601386a03e40c2940fcf17ccf9b4f1","b7e4933a2eb34f1d82e19d031f1f9707","1dc1fe7a1c344b289dcbada96268afb5","5474d2973eea4514a3361178941bc201","694caa51c54c440eb7d4beebd9f3b298","0e9770e6ac844fdf8d9b0a891ca8196a","540f86470cb2453ab41912682be092c0","a99b823388834f919717135bdf9790c0","2a6fcc73f7f84b848c9e08074db60931","9