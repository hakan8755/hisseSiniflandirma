score(y_true, y_pred, average=\"macro\")\n","    return acc, f1m\n","\n","TAB_COLS = feat_cols_amzn\n","X_tab = df_amzn[TAB_COLS].astype(\"float32\").values\n","y = df_amzn[\"y\"].values.astype(\"int64\")\n","\n","class TabDataset(Dataset):\n","    def __init__(self, X, y):\n","        self.X = torch.from_numpy(X)\n","        self.y = torch.from_numpy(y)\n","    def __len__(self): return len(self.y)\n","    def __getitem__(self, i):\n","        return self.X[i], self.y[i]\n","\n","class MLP(nn.Module):\n","    def __init__(self, in_dim, n_classes=3):\n","        super().__init__()\n","        self.net = nn.Sequential(\n","            nn.Linear(in_dim, 256),\n","            nn.ReLU(),\n","            nn.Dropout(0.2),\n","            nn.Linear(256, 128),\n","            nn.ReLU(),\n","            nn.Dropout(0.2),\n","            nn.Linear(128, n_classes),\n","        )\n","    def forward(self, x):\n","        return self.net(x)\n","\n","print(\"X_tab:\", X_tab.shape, \"| y:\", y.shape)\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"cOCtpc_PEALo","executionInfo":{"status":"ok","timestamp":1768504167080,"user_tz":-180,"elapsed":24,"user":{"displayName":"Hakan Aytuğ Fırat","userId":"02958237446288372335"}},"outputId":"14e3632c-3a28-4bf9-d57f-9c47f536c4ea"},"execution_count":38,"outputs":[{"output_type":"stream","name":"stdout","text":["device: cuda | gpu: NVIDIA A100-SXM4-80GB\n","X_tab: (2010, 17) | y: (2010,)\n"]}]},{"cell_type":"code","source":["EPOCHS = 20\n","BATCH = 128\n","LR = 1e-3\n","WD = 1e-4\n","\n","tab_accs, tab_f1s = [], []\n","\n","for fold_idx, (tr_idx, te_idx) in enumerate(folds_amzn, 1):\n","    Xtr, Xte = X_tab[tr_idx], X_tab[te_idx]\n","    ytr, yte = y[tr_idx], y[te_idx]\n","\n","    scaler = StandardScaler()\n","    Xtr = scaler.fit_transform(Xtr).astype(\"float32\")\n","    Xte = scaler.transform(Xte).astype(\"float32\")\n","\n","    train_loader = DataLoader(TabDataset(Xtr, ytr), batch_size=BATCH, shuffle=True)\n","    test_loader  = DataLoader(TabDataset(Xte, yte), batch_size=BATCH, shuffle=False)\n","\n","    model = MLP(in_dim=Xtr.shape[1]).to(device)\n","    opt = torch.optim.AdamW(model.parameters(), lr=LR, weight_decay=WD)\n","    loss_fn = nn.CrossEntropyLoss()\n","\n","    best_f1, patience, PATIENCE = -1, 0, 4\n","\n","    for ep in range(1, EPOCHS+1):\n","        model.train()\n","        total_loss = 0.0\n","        for xb, yb in train_loader:\n","            xb, yb = xb.to(device), yb.to(device)\n","            opt.zero_grad(set_to_none=True)\n","            logits = model(xb)\n","            loss = loss_fn(logits, yb)\n","            loss.backward()\n","            opt.step()\n","            total_loss += loss.item() * len(yb)\n","\n","        model.eval()\n","        preds, trues = [], []\n","        with torch.no_grad():\n","            for xb, yb in test_loader:\n","                xb = xb.to(device)\n","                p = torch.argmax(model(xb), dim=1).cpu().numpy()\n","                preds.extend(p.tolist())\n","                trues.extend(yb.numpy().tolist())\n","\n","        acc, f1m = compute_metrics(trues, preds)\n","\n","        if f1m > best_f1 + 1e-4:\n","            best_f1 = f1m\n","            patience = 0\n","            best_state = {k:v.detach().cpu().clone() for k,v in model.state_dict().items()}\n","        else:\n","            patience += 1\n","\n","        if ep % 2 == 0 or ep == 1:\n","            print(f\"[AMZN_TAB_MLP] Fold {fold_idx} | ep {ep:02d}/{EPOCHS} | loss={total_loss/len(tr_idx):.4f} | acc={acc:.3f} | macroF1={f1m:.3f}\")\n","\n","        if patience >= PATIENCE:\n","            break\n","\n","    model.load_state_dict(best_state)\n","\n","    model.eval()\n","    preds, trues = [], []\n","    with torch.no_grad():\n","        for xb, yb in test_loader:\n","            xb = xb.to(device)\n","            p = torch.argmax(model(xb), dim=1).cpu().numpy()\n","            preds.extend(p.tolist())\n","            trues.extend(yb.numpy().tolist())\n","\n","    acc, f1m = compute_metrics(trues, preds)\n","    tab_accs.append(acc); tab_f1s.append(f1m)\n","\n","    print(f\"\\n[AMZN_TAB_MLP] Fold {fold_idx} FINAL | Acc={acc:.3f} | MacroF1={f1m:.3f}\")\n","    print(confusion_matrix(trues, preds))\n","\n","print(\"\\n=== AMZN_TAB_MLP SUMMARY ===\")\n","print(\"Acc : %.3f ± %.3f\" % (np.mean(tab_accs), np.std(tab_accs)))\n","print(\"F1m : %.3f ± %.3f\" % (np.mean(tab_f1s), np.std(tab_f1s)))\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"xO1Ivz3JEBMc","executionInfo":{"status":"ok","timestamp":1768504176087,"user_tz":-180,"elapsed":1651,"user":{"displayName":"Hakan Aytuğ Fırat","userId":"02958237446288372335"}},"outputId":"687990d0-017b-4445-c1b6-e5826059e7a0"},"execution_count":39,"outputs":[{"output_type":"stream","name":"stdout","text":["[AMZN_TAB_MLP] Fold 1 | ep 01/20 | loss=1.0902 | acc=0.383 | macroF1=0.279\n","[AMZN_TAB_MLP] Fold 1 | ep 02/20 | loss=1.0787 | acc=0.405 | macroF1=0.318\n","[AMZN_TAB_MLP] Fold 1 | ep 04/20 | loss=1.0715 | acc=0.413 | macroF1=0.367\n","[AMZN_TAB_MLP] Fold 1 | ep 06/20 | loss=1.0692 | acc=0.418 | macroF1=0.366\n","[AMZN_TAB_MLP] Fold 1 | ep 08/20 | loss=1.0706 | acc=0.396 | macroF1=0.322\n","\n","[AMZN_TAB_MLP] Fold 1 FINAL | Acc=0.413 | MacroF1=0.367\n","[[56  8 75]\n"," [27 14 65]\n"," [50 11 96]]\n","[AMZN_TAB_MLP] Fold 2 | ep 01/20 | loss=1.0910 | acc=0.351 | macroF1=0.269\n","[AMZN_TAB_MLP] Fold 2 | ep 02/20 | loss=1.0824 | acc=0.356 | macroF1=0.328\n","[AMZN_TAB_MLP] Fold 2 | ep 04/20 | loss=1.0727 | acc=0.353 | macroF1=0.285\n","[AMZN_TAB_MLP] Fold 2 | ep 06/20 | loss=1.0673 | acc=0.358 | macroF1=0.299\n","\n","[AMZN_TAB_MLP] Fold 2 FINAL | Acc=0.356 | MacroF1=0.328\n","[[37 19 83]\n"," [27 21 58]\n"," [45 27 85]]\n","[AMZN_TAB_MLP] Fold 3 | ep 01/20 | loss=1.0958 | acc=0.396 | macroF1=0.264\n","[AMZN_TAB_MLP] Fold 3 | ep 02/20 | loss=1.0788 | acc=0.410 | macroF1=0.352\n","[AMZN_TAB_MLP] Fold 3 | ep 04/20 | loss=1.0666 | acc=0.408 | macroF1=0.366\n","[AMZN_TAB_MLP] Fold 3 | ep 06/20 | loss=1.0658 | acc=0.405 | macroF1=0.359\n","[AMZN_TAB_MLP] Fold 3 | ep 08/20 | loss=1.0639 | acc=0.396 | macroF1=0.334\n","\n","[AMZN_TAB_MLP] Fold 3 FINAL | Acc=0.408 | MacroF1=0.366\n","[[48 12 79]\n"," [27 17 62]\n"," [42 16 99]]\n","[AMZN_TAB_MLP] Fold 4 | ep 01/20 | loss=1.0865 | acc=0.386 | macroF1=0.287\n","[AMZN_TAB_MLP] Fold 4 | ep 02/20 | loss=1.0786 | acc=0.373 | macroF1=0.315\n","[AMZN_TAB_MLP] Fold 4 | ep 04/20 | loss=1.0624 | acc=0.358 | macroF1=0.310\n","[AMZN_TAB_MLP] Fold 4 | ep 06/20 | loss=1.0619 | acc=0.343 | macroF1=0.311\n","\n","[AMZN_TAB_MLP] Fold 4 FINAL | Acc=0.373 | MacroF1=0.315\n","[[44  7 88]\n"," [39  9 58]\n"," [51  9 97]]\n","[AMZN_TAB_MLP] Fold 5 | ep 01/20 | loss=1.0891 | acc=0.391 | macroF1=0.301\n","[AMZN_TAB_MLP] Fold 5 | ep 02/20 | loss=1.0809 | acc=0.403 | macroF1=0.335\n","[AMZN_TAB_MLP] Fold 5 | ep 04/20 | loss=1.0693 | acc=0.391 | macroF1=0.324\n","[AMZN_TAB_MLP] Fold 5 | ep 06/20 | loss=1.0685 | acc=0.408 | macroF1=0.344\n","[AMZN_TAB_MLP] Fold 5 | ep 08/20 | loss=1.0633 | acc=0.396 | macroF1=0.343\n","[AMZN_TAB_MLP] Fold 5 | ep 10/20 | loss=1.0636 | acc=0.393 | macroF1=0.329\n","\n","[AMZN_TAB_MLP] Fold 5 FINAL | Acc=0.396 | MacroF1=0.351\n","[[ 38  12  89]\n"," [ 29  18  59]\n"," [ 38  16 103]]\n","\n","=== AMZN_TAB_MLP SUMMARY ===\n","Acc : 0.389 ± 0.022\n","F1m : 0.346 ± 0.021\n"]}]},{"cell_type":"code","source":["!pip -q install -U transformers accelerate\n","\n","import torch\n","import torch.nn as nn\n","from torch.utils.data import Dataset, DataLoader\n","from transformers import AutoTokenizer, AutoModel, get_linear_schedule_with_warmup\n","from tqdm.auto import tqdm\n","import numpy as np\n","from sklearn.metrics import accuracy_score, f1_score\n","\n","MODEL_NAME = \"distilbert-base-uncased\"\n","tokenizer = AutoTokenizer.from_pretrained(MODEL_NAME)\n","\n","MAX_TOKENS = 192\n","class TextTfDataset(Dataset):\n","    def __init__(self, texts, y):\n","        self.texts = texts\n","        self.y = y\n","    def __len__(self): return len(self.y)\n","    def __getitem__(self, i):\n","        enc = tokenizer(\n","            self.texts[i],\n","            truncation=True,\n","            padding=\"max_length\",\n","            max_length=MAX_TOKENS,\n","            return_tensors=\"pt\"\n","        )\n","        item = {k: v.squeeze(0) for k,v in enc.items()}\n","        item[\"y\"] = torch.tensor(self.y[i], dtype=torch.long)\n","        return item\n","\n","class DistilBertClassifier(nn.Module):\n","    def __init__(self, base, hidden=768, n_classes=3, dropout=0.2):\n","        super().__init__()\n","        self.base = base\n","        self.drop = nn.Dropout(dropout)\n","        self.fc = nn.Linear(hidden, n_classes)\n","    def forward(self, input_ids, attention_mask):\n","        out = self.base(input_ids=input_ids, attention_mask=attention_mask)\n","        cls = out.last_hidden_state[:, 0, :]\n","        return self.fc(self.drop(cls))\n","\n","def metrics(y_true, y_pred):\n","    return accuracy_score(y_true, y_pred), f1_score(y_true, y_pred, average=\"macro\")\n","\n","EPOCHS = 3\n","BATCH = 32\n","LR = 2e-5\n","WD = 0.01\n","\n","distil_accs, distil_f1s = [], []\n","\n","for fold_idx, (tr_idx, te_idx) in enumerate(folds_amzn, 1):\n","    print(f\"\\n===== AMZN_TEXT_DISTILBERT | Fold {fold_idx} =====\")\n","\n","    tr_texts = df_amzn[\"text\"].iloc[tr_idx].tolist()\n","    te_texts = df_amzn[\"text\"].iloc[te_idx].tolist()\n","    ytr = df_amzn[\"y\"].iloc[tr_idx].values\n","    yte = df_amzn[\"y\"].iloc[te_idx].values\n","\n","    train_loader = DataLoader(TextTfDataset(tr_texts, ytr), batch_size=BATCH, shuffle=True)\n","    test_loader  = DataLoader(TextTfDataset(te_texts, yte), batch_size=BATCH, shuffle=False)\n","\n","    model = DistilBertClassifier(AutoModel.from_pretrained(MODEL_NAME)).to(device)\n","    opt = torch.optim.AdamW(model.parameters(), lr=LR, weight_decay=WD)\n","    total_steps = EPOCHS * len(train_loader)\n","    sched = get_linear_schedule_with_warmup(opt, num_warmup_steps=int(0.1*total_steps), num_training_steps=total_steps)\n","\n","    loss_fn = nn.CrossEntropyLoss()\n","    scaler = torch.cuda.amp.GradScaler(enabled=(device==\"cuda\"))\n","\n","    best_f1, patience, PATIENCE = -1, 0, 2\n","\n","    for ep in range(1, EPOCHS+1):\n","        model.train()\n","        total_loss = 0.0\n","        for batch in tqdm(train_loader, desc=f\"Fold{fold_idx} ep{ep}\", leave=False):\n","            input_ids = batch[\"input_ids\"].to(device)\n","            attention_mask = batch[\"attention_mask\"].to(device)\n","            yb = batch[\"y\"].to(device)\n","\n","            opt.zero_grad(set_to_none=True)\n","            with torch.cuda.amp.autocast(enabled=(device==\"cuda\")):\n","                logits = model(input_ids, attention_mask)\n","                loss = loss_fn(logits, yb)\n","\n","            scaler.scale(loss).backward()\n","            scaler.step(opt)\n","            scaler.update()\n","            sched.step()\n","\n","            total_loss += loss.item() * len(yb)\n","\n","        model.eval()\n","        preds, trues = [], []\n","        with torch.no_grad():\n","            for batch in test_loader:\n","                input_ids = batch[\"input_ids\"].to(device)\n","                attention_mask = batch[\"attention_mask\"].to(device)\n","                logits = model(input_ids, attention_mask)\n","                p = torch.argmax(logits, dim=1).cpu().numpy()\n","                preds.extend(p.tolist())\n","                trues.extend(batch[\"y\"].numpy().tolist())\n","\n","        acc, f1m = metrics(trues, preds)\n","        print(f\"[AMZN_TEXT_DISTIL] ep {ep:02d}/{EPOCHS} | loss={total_loss/len(tr_idx):.4f} | acc={acc:.3f} | macroF1={f1m:.3f}\")\n","\n","        if f1m > best_f1 + 1e-4:\n","            best_f1 = f1m\n","            patience = 0\n","            best_state = {k:v.detach().cpu().clone() for k,v in model.state_dict().items()}\n","        else:\n","            patience += 1\n","            if patience >= PATIENCE:\n","                break\n","\n","    model.load_state_dict(best_state)\n","\n","    model.eval()\n","    preds, trues = [], []\n","    with torch.no_grad():\n","        for batch in test_loader:\n","            input_ids = batch[\"input_ids\"].to(device)\n","            attention_mask = batch[\"attention_mask\"].to(device)\n","            logits = model(input_ids, attention_mask)\n","            p = torch.argmax(logits, dim=1).cpu().numpy()\n","            preds.extend(p.tolist())\n","            trues.extend(batch[\"y\"].numpy().tolist())\n","\n","    acc, f1m = metrics(trues, preds)\n","    distil_accs.append(acc); distil_f1s.append(f1m)\n","\n","    print(f\"[AMZN_TEXT_DISTIL] Fold {fold_idx} FINAL | Acc={acc:.3f} | MacroF1={f1m:.3f}\")\n","\n","print(\"\\n=== AMZN_TEXT_DISTILBERT SUMMARY ===\")\n","print(\"Acc : %.3f ± %.3f\" % (np.mean(distil_accs), np.std(distil_accs)))\n","print(\"F1m : %.3f ± %.3f\" % (np.mean(distil_f1s), np.std(distil_f1s)))\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":1000,"referenced_widgets":["2b7531aa389843b5ab45feb36f6d1e18","26d0403e9bca4cff9bafc271301f36ce","349de3d1d7f646a1baf399c6fda77b47","df6ef96ace0b4838b05109541d51721c","1eebf8d5d5ae4409a5be42d423801dc9","efbc870af56d4b47b30111d157a9c682","3e9d56695b644d97bb0af11598f2df7d","4f05e28075f0447e86f52ba6028d0ab0","182f034c26c04c419dd6e12727a4bb8a","aed06320327e4b308e0d6f65b8f4280b","51004f51879649448f48368f675f0f59","a929ab8256c145f6836657f201c48f28","686eedc57fca406388b3ca534d57613c","e76cc1a6ee9c4332abdd11a9a42779e7","546d5ab003e741a4b2287046248bca48","0ff70bd3340e45029db7c3809eaab30c","d4bd53c1436345eabeb49dcf7949b275","4e0e10f59ed141d899c8cd5bceacd47f","f1b95d86c2484b788169950119772035","fcbf3558b337473f88b090a083c38375","4fcb68098e8e4003a94dfedeea7697df","af13d60ea2d14de7be19490f740e01ed","d6b5be9ecda2415aa6ade14d6916cec6","70eee3417ce7464c93b5064705515186","f370ec8c347d4