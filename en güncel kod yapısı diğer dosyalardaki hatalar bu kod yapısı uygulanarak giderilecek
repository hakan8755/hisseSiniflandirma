d. Please use `torch.amp.autocast('cuda', args...)` instead.\n","  with torch.cuda.amp.autocast(enabled=(device==\"cuda\")):\n"]},{"output_type":"stream","name":"stdout","text":["[AMZN_TEXT_DISTIL] ep 02/3 | loss=1.0856 | acc=0.388 | macroF1=0.237\n"]},{"output_type":"display_data","data":{"text/plain":["Fold5 ep3:   0%|          | 0/51 [00:00<?, ?it/s]"],"application/vnd.jupyter.widget-view+json":{"version_major":2,"version_minor":0,"model_id":"e1ff8e22dffa4d55b72b6859ffef7cee"}},"metadata":{}},{"output_type":"stream","name":"stderr","text":["/tmp/ipython-input-659031798.py:83: FutureWarning: `torch.cuda.amp.autocast(args...)` is deprecated. Please use `torch.amp.autocast('cuda', args...)` instead.\n","  with torch.cuda.amp.autocast(enabled=(device==\"cuda\")):\n"]},{"output_type":"stream","name":"stdout","text":["[AMZN_TEXT_DISTIL] ep 03/3 | loss=1.0765 | acc=0.391 | macroF1=0.287\n","[AMZN_TEXT_DISTIL] Fold 5 FINAL | Acc=0.391 | MacroF1=0.287\n","\n","=== AMZN_TEXT_DISTILBERT SUMMARY ===\n","Acc : 0.379 ± 0.013\n","F1m : 0.282 ± 0.016\n"]}]},{"cell_type":"code","source":["!pip -q install -U transformers accelerate\n","\n","import torch\n","import torch.nn as nn\n","from torch.utils.data import Dataset, DataLoader\n","from transformers import AutoTokenizer, AutoModel, get_linear_schedule_with_warmup\n","from tqdm.auto import tqdm\n","import numpy as np\n","from sklearn.metrics import accuracy_score, f1_score\n","\n","MODEL_NAME = \"distilbert-base-uncased\"\n","tokenizer = AutoTokenizer.from_pretrained(MODEL_NAME)\n","\n","MAX_TOKENS = 192\n","class TextTfDataset(Dataset):\n","    def __init__(self, texts, y):\n","        self.texts = texts\n","        self.y = y\n","    def __len__(self): return len(self.y)\n","    def __getitem__(self, i):\n","        enc = tokenizer(\n","            self.texts[i],\n","            truncation=True,\n","            padding=\"max_length\",\n","            max_length=MAX_TOKENS,\n","            return_tensors=\"pt\"\n","        )\n","        item = {k: v.squeeze(0) for k,v in enc.items()}\n","        item[\"y\"] = torch.tensor(self.y[i], dtype=torch.long)\n","        return item\n","\n","class DistilBertClassifier(nn.Module):\n","    def __init__(self, base, hidden=768, n_classes=3, dropout=0.2):\n","        super().__init__()\n","        self.base = base\n","        self.drop = nn.Dropout(dropout)\n","        self.fc = nn.Linear(hidden, n_classes)\n","    def forward(self, input_ids, attention_mask):\n","        out = self.base(input_ids=input_ids, attention_mask=attention_mask)\n","        cls = out.last_hidden_state[:, 0, :]\n","        return self.fc(self.drop(cls))\n","\n","def metrics(y_true, y_pred):\n","    return accuracy_score(y_true, y_pred), f1_score(y_true, y_pred, average=\"macro\")\n","\n","EPOCHS = 3\n","BATCH = 32\n","LR = 2e-5\n","WD = 0.01\n","\n","distil_accs, distil_f1s = [], []\n","\n","for fold_idx, (tr_idx, te_idx) in enumerate(folds_amzn, 1):\n","    print(f\"\\n===== AMZN_TEXT_DISTILBERT | Fold {fold_idx} =====\")\n","\n","    tr_texts = df_amzn[\"text\"].iloc[tr_idx].tolist()\n","    te_texts = df_amzn[\"text\"].iloc[te_idx].tolist()\n","    ytr = df_amzn[\"y\"].iloc[tr_idx].values\n","    yte = df_amzn[\"y\"].iloc[te_idx].values\n","\n","    train_loader = DataLoader(TextTfDataset(tr_texts, ytr), batch_size=BATCH, shuffle=True)\n","    test_loader  = DataLoader(TextTfDataset(te_texts, yte), batch_size=BATCH, shuffle=False)\n","\n","    model = DistilBertClassifier(AutoModel.from_pretrained(MODEL_NAME)).to(device)\n","    opt = torch.optim.AdamW(model.parameters(), lr=LR, weight_decay=WD)\n","    total_steps = EPOCHS * len(train_loader)\n","    sched = get_linear_schedule_with_warmup(opt, num_warmup_steps=int(0.1*total_steps), num_training_steps=total_steps)\n","\n","    loss_fn = nn.CrossEntropyLoss()\n","    scaler = torch.cuda.amp.GradScaler(enabled=(device==\"cuda\"))\n","\n","    best_f1, patience, PATIENCE = -1, 0, 2\n","\n","    for ep in range(1, EPOCHS+1):\n","        model.train()\n","        total_loss = 0.0\n","        for batch in tqdm(train_loader, desc=f\"Fold{fold_idx} ep{ep}\", leave=False):\n","            input_ids = batch[\"input_ids\"].to(device)\n","            attention_mask = batch[\"attention_mask\"].to(device)\n","            yb = batch[\"y\"].to(device)\n","\n","            opt.zero_grad(set_to_none=True)\n","            with torch.cuda.amp.autocast(enabled=(device==\"cuda\")):\n","                logits = model(input_ids, attention_mask)\n","                loss = loss_fn(logits, yb)\n","\n","            scaler.scale(loss).backward()\n","            scaler.step(opt)\n","            scaler.update()\n","            sched.step()\n","\n","            total_loss += loss.item() * len(yb)\n","\n","        model.eval()\n","        preds, trues = [], []\n","        with torch.no_grad():\n","            for batch in test_loader:\n","                input_ids = batch[\"input_ids\"].to(device)\n","                attention_mask = batch[\"attention_mask\"].to(device)\n","                logits = model(input_ids, attention_mask)\n","                p = torch.argmax(logits, dim=1).cpu().numpy()\n","                preds.extend(p.tolist())\n","                trues.extend(batch[\"y\"].numpy().tolist())\n","\n","        acc, f1m = metrics(trues, preds)\n","        print(f\"[AMZN_TEXT_DISTIL] ep {ep:02d}/{EPOCHS} | loss={total_loss/len(tr_idx):.4f} | acc={acc:.3f} | macroF1={f1m:.3f}\")\n","\n","        if f1m > best_f1 + 1e-4:\n","            best_f1 = f1m\n","            patience = 0\n","            best_state = {k:v.detach().cpu().clone() for k,v in model.state_dict().items()}\n","        else:\n","            patience += 1\n","            if patience >= PATIENCE:\n","                break\n","\n","    model.load_state_dict(best_state)\n","\n","    model.eval()\n","    preds, trues = [], []\n","    with torch.no_grad():\n","        for batch in test_loader:\n","            input_ids = batch[\"input_ids\"].to(device)\n","            attention_mask = batch[\"attention_mask\"].to(device)\n","            logits = model(input_ids, attention_mask)\n","            p = torch.argmax(logits, dim=1).cpu().numpy()\n","            preds.extend(p.tolist())\n","            trues.extend(batch[\"y\"].numpy().tolist())\n","\n","    acc, f1m = metrics(trues, preds)\n","    distil_accs.append(acc); distil_f1s.append(f1m)\n","\n","    print(f\"[AMZN_TEXT_DISTIL] Fold {fold_idx} FINAL | Acc={acc:.3f} | MacroF1={f1m:.3f}\")\n","\n","print(\"\\n=== AMZN_TEXT_DISTILBERT SUMMARY ===\")\n","print(\"Acc : %.3f ± %.3f\" % (np.mean(distil_accs), np.std(distil_accs)))\n","print(\"F1m : %.3f ± %.3f\" % (np.mean(distil_f1s), np.std(distil_f1s)))\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":1000,"referenced_widgets":["3f5c5aeb48fb4b21a357508cef5d9f63","fdd3b187e17143b09120ef230097c402","4625cf625b3e4549915df46d8ea1b028","385401d89c244e40bce799c89df2d18a","53a5c009a2d64bc6926c318f9270dbf7","c9b905e9a608422ab2745b2dda5dbfb8","fee633dfb7c74af6a3be4ebb740b33d7","e901cc4f34e049068f74db50b3754c9a","d977108173694f7aaa2d47ea9d372251","38776dee704d473dbe0e1c36b6ea97a8","939f3616252642b3b113e6db87651637","cf2e9f65a2214dc28996f542c17d4a2c","6c5f682aaeee4e87884b736a6fe935ae","ac5ef26222914ef0b704c24987f9ffd4","e7345b4053664ee6bb230939e21d7e2a","340b56606a9442a9ad381d7e10c8ba03","c8eaa3d6cac5488084f962c16a014847","258bb7192464472b8ca0613c5493fac0","1b97f8f1ddcc4003ab894120eab1d58c","5550f1524c9d499cb7a0b4dd93ceb29f","414a9a380c9f459695f84dd412c9b1af","8ff3f179f9f640a4b573afe48d856c55","21b4ef7493ed479ab91fc92b2e17c740","d158fa868af14630b35e9614714c7a07","25bb6b8a674c42c1a2b81831242853e8","7179d931e341438d8963a0454e7d311d","370c6ff539834605b0762ce6e6d75358","0b217af9c31a4fe59a17088b711dfc3b","00692752667044af8b825b2bdd31b4cc","75573ba76eeb4c77984bdbb8916a52da","cc6dd79e72aa4f4b929f9031ee50b9c9","23875e0aadee47f0881f8fe0fddc0454","5be7d949551f49e1a5c717ee3fcbe94c","c6b681374cfb45859e9369a299c2cf69"