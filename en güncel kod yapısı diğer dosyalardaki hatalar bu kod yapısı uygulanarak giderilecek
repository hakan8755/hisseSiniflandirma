MacroF1=0.365\n","[[72  5 62]\n"," [41 10 54]\n"," [63  7 86]]\n","\n","=== TS_TRANS1 SUMMARY ===\n","Acc : 0.395 ± 0.016\n","F1m : 0.371 ± 0.016\n"]}]},{"cell_type":"code","source":["import numpy as np\n","import torch\n","import torch.nn as nn\n","\n","class PositionalEncoding(nn.Module):\n","    def __init__(self, d_model, max_len=512):\n","        super().__init__()\n","        pe = torch.zeros(max_len, d_model)\n","        position = torch.arange(0, max_len, dtype=torch.float).unsqueeze(1)\n","        div = torch.exp(torch.arange(0, d_model, 2).float() * (-np.log(10000.0) / d_model))\n","        pe[:, 0::2] = torch.sin(position * div)\n","        pe[:, 1::2] = torch.cos(position * div)\n","        self.register_buffer(\"pe\", pe.unsqueeze(0))\n","\n","    def forward(self, x):\n","        T = x.size(1)\n","        return x + self.pe[:, :T, :]\n","\n","class TCNTransformer(nn.Module):\n","    \"\"\"\n","    Hybrid:\n","    (B,T,F) -> Conv1D over time -> TransformerEncoder -> last token pooling -> cls\n","    \"\"\"\n","    def __init__(self, n_feat, d_model=128, n_heads=4, n_layers=2, dropout=0.2, n_classes=3):\n","        super().__init__()\n","\n","        # 1) project feat -> d_model\n","        self.proj = nn.Linear(n_feat, d_model)\n","\n","        # 2) temporal conv block (TCN-ish)\n","        # conv expects (B, C, T)\n","        self.conv = nn.Sequential(\n","            nn.Conv1d(d_model, d_model, kernel_size=3, padding=1),\n","            nn.GELU(),\n","            nn.Dropout(dropout),\n","            nn.Conv1d(d_model, d_model, kernel_size=3, padding=1),\n","            nn.GELU(),\n","            nn.Dropout(dropout),\n","        )\n","        self.norm = nn.LayerNorm(d_model)\n","\n","        # 3) transformer\n","        self.pos = PositionalEncoding(d_model, max_len=512)\n","        enc_layer = nn.TransformerEncoderLayer(\n","            d_model=d_model,\n","            nhead=n_heads,\n","            dim_feedforward=4*d_model,\n","            dropout=dropout,\n","            batch_first=True,\n","            activation=\"gelu\",\n","            norm_first=False  # burada False yapıyoruz -> warning da kesilir\n","        )\n","        self.encoder = nn.TransformerEncoder(enc_layer, num_layers=n_layers)\n","\n","        # 4) classifier\n","        self.drop = nn.Dropout(dropout)\n","        self.fc = nn.Linear(d_model, n_classes)\n","\n","    def forward(self, x):\n","        x = self.proj(x)          # (B,T,d)\n","        # conv over time\n","        h = x.transpose(1, 2)     # (B,d,T)\n","        h = self.conv(h)          # (B,d,T)\n","        h = h.transpose(1, 2)     # (B,T,d)\n","        h = self.norm(h)\n","\n","        h = self.pos(h)\n","        h = self.encoder(h)       # (B,T,d)\n","        last = h[:, -1, :]\n","        return self.fc(self.drop(last))\n"],"metadata":{"id":"cOSHLFWwJbMT","executionInfo":{"status":"ok","timestamp":1768505589313,"user_tz":-180,"elapsed":31,"user":{"displayName":"Hakan Aytuğ Fırat","userId":"02958237446288372335"}}},"execution_count":49,"outputs":[]},{"cell_type":"code","source":["import numpy as np\n","import torch\n","import torch.nn as nn\n","from torch.utils.data import DataLoader\n","from sklearn.preprocessing import StandardScaler\n","from sklearn.metrics import confusion_matrix\n","\n","EPOCHS = 30\n","BATCH = 256\n","LR = 2e-4\n","WD = 1e-4\n","PATIENCE = 5\n","\n","tt2_accs, tt2_f1s = [], []\n","\n","for fold_idx, (tr_i, te_i) in enumerate(folds_ts, 1):\n","    Xtr, Xte = Xw[tr_i].copy(), Xw[te_i].copy()\n","    ytr, yte = yw[tr_i].copy(), yw[te_i].copy()\n","\n","    # --- scale (fit train only) ---\n","    scaler = StandardScaler()\n","    Xtr2 = Xtr.reshape(-1, Xtr.shape[-1])\n","    Xte2 = Xte.reshape(-1, Xte.shape[-1])\n","    Xtr2 = scaler.fit_transform(Xtr2).astype(\"float32\")\n","    Xte2 = scaler.transform(Xte2).astype(\"float32\")\n","    Xtr = Xtr2.reshape(Xtr.shape)\n","    Xte = Xte2.reshape(Xte.shape)\n","\n","    train_loader = DataLoader(SeqDataset(Xtr, ytr), batch_size=BATCH, shuffle=True)\n","    test_loader  = DataLoader(SeqDataset(Xte, yte), batch_size=BATCH, shuffle=False)\n","\n","    model = TCNTransformer(n_feat=Xtr.shape[-1], d_model=128, n_heads=4, n_layers=2, dropout=0.2).to(device)\n","    opt = torch.optim.AdamW(model.parameters(), lr=LR, weight_decay=WD)\n","    loss_fn = nn.CrossEntropyLoss()\n","\n","    best_f1, patience = -1, 0\n","    best_state = None\n","\n","    for ep in range(1, EPOCHS+1):\n","        model.train()\n","        total_loss = 0.0\n","\n","        for xb, yb in train_loader:\n","            xb, yb = xb.to(device), yb.to(device)\n","            opt.zero_grad(set_to_none=True)\n","            logits = model(xb)\n","            loss = loss_fn(logits, yb)\n","            loss.backward()\n","            torch.nn.utils.clip_grad_norm_(model.parameters(), 1.0)\n","            opt.step()\n","            total_loss += loss.item() * len(yb)\n","\n","        # eval\n","        model.eval()\n","        preds, trues = [], []\n","        with torch.no_grad():\n","            for xb, yb in test_loader:\n","                xb = xb.to(device)\n","                p = torch.argmax(model(xb), dim=1).cpu().numpy()\n","                preds.extend(p.tolist())\n","                trues.extend(yb.numpy().tolist())\n","\n","        acc, f1m = metrics(trues, preds)\n","\n","        if ep == 1 or ep % 2 == 0:\n","            print(f\"[TS_TRANS2] Fold {fold_idx} | ep {ep:02d}/{EPOCHS} | loss={total_loss/len(tr_i):.4f} | acc={acc:.3f} | macroF1={f1m:.3f}\")\n","\n","        if f1m > best_f1 + 1e-4:\n","            best_f1 = f1m\n","            patience = 0\n","            best_state = {k:v.detach().cpu().clone() for k,v in model.state_dict().items()}\n","        else:\n","            patience += 1\n","            if patience >= PATIENCE:\n","                break\n","\n","    model.load_state_dict(best_state)\n","\n","    # final\n","    model.eval()\n","    preds, trues = [], []\n","    with torch.no_grad():\n","        for xb, yb in test_loader:\n","            xb = xb.to(device)\n","            p = torch.argmax(model(xb), dim=1).cpu().numpy()\n","            preds.extend(p.tolist())\n","            trues.extend(yb.numpy().tolist())\n","\n","    acc, f1m = metrics(trues, preds)\n","    tt2_accs.append(acc); tt2_f1s.append(f1m)\n","\n","    print(f\"\\n[TS_TRANS2] Fold {fold_idx} FINAL | Acc={acc:.3f} | MacroF1={f1m:.3f}\")\n","    print(confusion_matrix(trues, preds))\n","\n","print(\"\\n=== TS_TRANS2 SUMMARY ===\")\n","print(\"Acc : %.3f ± %.3f\" % (np.mean(tt2_accs), np.std(tt2_accs)))\n","print(\"F1m : %.3f ± %.3f\" % (np.mean(tt2_f1s), np.std(tt2_f1s)))\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"BnQO2FHzJLe6","executionInfo":{"status":"ok","timestamp":1768505604010,"user_tz":-180,"elapsed":6315,"user":{"displayName":"Hakan Aytuğ Fırat","userId":"02958237446288372335"}},"outputId":"5bef2dd8-32d8-4dc1-c427-23a770b91826"},"execution_count":50,"outputs":[{"output_type":"stream","name":"stdout","text":["[TS_TRANS2] Fold 1 | ep 01/30 | loss=1.1855 | acc=0.404 | macroF1=0.329\n","[TS_TRANS2] Fold 1 | ep 02/30 | loss=1.1201 | acc=0.383 | macroF1=0.309\n","[TS_TRANS2] Fold 1 | ep 04/30 | loss=1.1005 | acc=0.396 | macroF1=0.331\n","[TS_TRANS2] Fold 1 | ep 06/30 | loss=1.0941 | acc=0.409 | macroF1=0.337\n","[TS_TRANS2] Fold 1 | ep 08/30 | loss=1.0949 | acc=0.401 | macroF1=0.349\n","\n","[TS_TRANS2] Fold 1 FINAL | Acc=0.398 | MacroF1=0.356\n","[[50  9 80]\n"," [29 15 61]\n"," [45 16 94]]\n","[TS_TRANS2] Fold 2 | ep 01/30 | loss=1.1497 | acc=0.314 | macroF1=0.309\n","[TS_TRANS2] Fold 2 | ep 02/30 | loss=1.1288 | acc=0.395 | macroF1=0.315\n","[TS_TRANS2] Fold 2 | ep 04/30 | loss=1.1114 | acc=0.372 | macroF1=0.286\n","[TS_TRANS2] Fold 2 | ep 06/30 | loss=1.1003 | acc=0.352 | macroF1=0.294\n","[TS_TRANS2] Fold 2 | ep 08/30 | loss=1.0819 | acc=0.372 | macroF1=0.335\n","[TS_TRANS2] Fold 2 | ep 10/30 | loss=1.0855 | acc=0.339 | macroF1=0.326\n","[TS_TRANS2] Fold 2 | ep 12/30 | loss=1.0817 | acc=0.377 | macroF1=0.369\n","[TS_TRANS2] Fold 2 | ep 14/30 | loss=1.0697 | acc=0.382 | macroF1=0.334\n","[TS_TRANS2] Fold 2 | ep 16/30 | loss=1.0783 | acc=0.377 | macroF1=0.325\n","\n","[TS_TRANS2] Fold 2 FINAL | Acc=0.377 | MacroF1=0.369\n","[[43 20 76]\n"," [31 33 40]\n"," [46 33 73]]\n","[TS_TRANS2] Fold 3 | ep 01/30 | loss=1.1446 | acc=0.356 | macroF1=0.326\n","[TS_TRANS2] Fold 3 | ep 02/30 | loss=1.1114 | acc=0.401 | macroF1=0.353\n","[TS_TRANS2] Fold 3 | ep 04/30 | loss=1.0997 | acc=0.373 | macroF1=0.362\n","[TS_TRANS2] Fold 3 | ep 06/30 | loss=1.0944 | acc=0.378 | macroF1=0.363\n","[TS_TRANS2] Fold 3 | ep 08/30 | loss=1.0763 | acc=0.378 | macroF1=0.318\n","[TS_TRANS2] Fold 3 | ep 10/30 | loss=1.0753 | acc=0.398 | macroF1=0.369\n","[TS_TRANS2] Fold 3 | ep 12/30 | loss=1.0785 | acc=0.393 | macroF1=0.380\n","[TS_TRANS2] Fold 3 | ep 14/30 | loss=1.0684 | acc=0.383 | macroF1=0.338\n","[TS_TRANS2] Fold 3 | ep 16/30 | loss=1.0663 | acc=0.396 | macroF1=0.353\n","\n","[TS_TRANS2] Fold 3 FINAL | Acc=0.393 | MacroF1=0.380\n","[[56 13 69]\n"," [41 26 38]\n"," [62 19 75]]\n","[TS_TRANS2] Fold 4 | ep 01/30 | loss=1.1764 | acc=0.354 | macroF1=0.264\n","[TS_TRANS2] Fold 4 | ep 02/30 | loss=1.1258 | acc=0.369 | macroF1=0.318\n","[TS_TRANS2] Fold 4 | ep 04/30 | loss=1.1082 | acc=0.399 | macroF1=0.282\n","[TS_TRANS2] Fold 4 | ep 06/30 | loss=1.0869 | acc=0.362 | macroF1=0.309\n","\n","[TS_TRANS2] Fold 4 FINAL | Acc=0.369 | MacroF1=0.318\n","[[ 17  35  87]\n"," [ 10  27  67]\n"," [ 18  34 103]]\n","[TS_TRANS2] Fold 5 | ep 01/30 | loss=1.2020 | acc=0.340 | macroF1=0.249\n","[TS_TRANS2] Fold 5 | ep 02/30 | loss=1.1384 | acc=0.370 | macroF1=0.306\n","[TS_TRANS2] Fold 5 | ep 04/30 | loss=1.1097 | acc=0.350 | macroF1=0.335\n","[TS_TRANS2] Fold 5 | ep 06/30 | loss=1.0975 | acc=0.350 | macroF1=0.330\n","[TS_TRANS2] Fold 5 | ep 08/30 | loss=1.0831 | acc=0.352 | macroF1=0.295\n","\n","[TS_TRANS2] Fold 5 FINAL | Acc=0.350 | MacroF1=0.335\n","[[49 28 62]\n"," [32 23 50]\n"," [64 24 68]]\n","\n","=== TS_TRANS2 SUMMARY ===\n","Acc : 0.378 ± 0.017\n","F1m : 0.352 ± 0.022\n"]}]},{"cell_type":"code","source":["import numpy as np\n","from sklearn.metrics import accuracy_score, f1_score, confusion_matrix\n","\n","def softmax_np(logits):\n","    logits = logits - logits.max(axis=1, keepdims=True)\n","    exp = np.exp(logits)\n","    return exp / exp.sum(axis=1, keepdims=True)\n","\n","def eval_from_proba(y_true, proba):\n","    pred = np.argmax(proba, axis=1)\n","    acc = accuracy_score(y_true, pred)\n","    f1m = f1_score(y_true, pred, average=\"macro\")\n","    return acc, f1m, confusion_matrix(y_true, pred)\n","\n","def late_fusion_avg(p_text, p_time):\n","    return (p_text + p_time) / 2.0\n","\n","def late_fusion_weighted(p_text, p_time, alpha=0.5):\n","    # alpha -> time weight\n","    return alpha*p_time + (1.0-alpha)*p_text\n","\n","def late_fusion_vote(p_text, p_time):\n","    # hard vote with tie-break = time\n","    t = np.argmax(p_text, axis=1)\n","    s = np.argmax(p_time, axis=1)\n","    out = []\n","    for i in range(len(t)):\n","        if t[i] == s[i]:\n","            out.append(t[i])\n","        else:\n","            out.append(s[i])  # tie -> time\n","    out = np.array(out)\n","    # convert to one-hot proba for eval convenience\n","    proba = np.zeros((len(out), 3), dtype=np.float32)\n","    proba[np.arange(len(out)), out] = 1.0\n","    return proba\n"],"metadata":{"id":"6aakii8vKA8e","executionInfo":{"status":"ok","timestamp":1768505743352,"user_tz":-180,"elapsed":30,"user":{"displayName":"Hakan Aytuğ Fırat","userId":"02958237446288372335"}}},"execution_count":52,"outputs":[]},{"cell_type":"code","source":["import torch\n","import torch.nn as nn\n","from torch.utils.data import Dataset, DataLoader\n","from sklearn.preprocessing import StandardScaler\n","\n","device = \"cuda\" if torch.cuda.is_available() else \"cpu\"\n","\n","class SeqDataset(Dataset):\n","    def __init__(self, Xw, y):\n","        self.X = torch.from_numpy(Xw)  # (N,T,F)\n","        self.y = torch.from_numpy(y)\n","    def __len__(self): return len(self.y)\n","    def __getitem__(self, i):\n","        return self.X[i], self.y[i]\n","\n","class LSTMClassifier(nn.Module):\n","    def __init__(self, n_feat, hidden=128, n_layers=2, dropout=0.2, n_classes=3):\n","        super().__init__()\n","        self.lstm = nn.LSTM(\n","            input_size=n_feat,\n","            hidden_size=hidden,\n","            num_layers=n_layers,\n","            batch_first=True,\n","            dropout=dropout if n_layers > 1 else 0.0,\n","            bidirectional=False\n","        )\n","        self.drop = nn.Dropout(dropout)\n","        self.fc = nn.Linear(hidden, n_classes)\n","\n","    def forward(self, x):\n","        _, (hn, _) = self.lstm(x)\n","        last = hn[-1]\n","        return self.fc(self.drop(last))\n","\n","def train_ts_lstm_and_proba(Xw, yw, folds_ts, epochs=20, batch=256, lr=1e-3, wd=1e-4, patience=5):\n","    all_test_proba = [None]*len(folds_ts)\n","    all_test_y = [None]*len(folds_ts)\n","\n","    for fold_idx, (tr_i, te_i) in enumerate(folds_ts, 1):\n","        Xtr, Xte = Xw[tr_i].copy(), Xw[te_i].copy()\n","        ytr, yte = yw[tr_i].copy(), yw[te_i].copy()\n","\n","        scaler = StandardScaler()\n","        Xtr2 = scaler.fit_transform(Xtr.reshape(-1, Xtr.shape[-1])).astype(\"float32\")\n","        Xte2 = scaler.transform(Xte.reshape(-1, Xte.shape[-1])).astype(\"float32\")\n","        Xtr = Xtr2.reshape(Xtr.shape)\n","        Xte = Xte2.reshape(Xte.shape)\n","\n","        train_loader = DataLoader(SeqDataset(Xtr, ytr), batch_size=batch, shuffle=True)\n","        test_loader  = DataLoader(SeqDataset(Xte, yte), batch_size=batch, shuffle=False)\n","\n","        model = LSTMClassifier(n_feat=Xtr.shape[-1]).to(device)\n","        opt = torch.optim.AdamW(model.parameters(), lr=lr, weight_decay=wd)\n","        loss_fn = nn.CrossEntropyLoss()\n","\n","        best_f1, bad = -1, 0\n","        best_state = None\n","\n","        for ep in range(1, epochs+1):\n","            model.train()\n","            for xb, yb in train_loader:\n","                xb, yb = xb.to(device), yb.to(device)\n","                opt.zero_grad(set_to_none=True)\n","                logits = model(xb)\n","                loss = loss_fn(logits, yb)\n","                loss.backward()\n","                opt.step()\n","\n","            # eval for early stop\n","            model.eval()\n","            preds, trues = [], []\n","            with torch.no_grad():\n","                for xb, yb in test_loader:\n","                    xb = xb.to(device)\n","                    p = torch.argmax(model(xb), dim=1).cpu().numpy()\n","                    preds.extend(p.tolist())\n","                    trues.extend(yb.numpy().tolist())\n","            f1m = f1_score(trues, preds, average=\"macro\")\n","\n","            if f1m > best_f1 + 1e-4:\n","                best_f1 = f1m\n","                bad = 0\n","                best_state = {k:v.detach().cpu().clone() for k,v in model.state_dict().items()}\n","            else:\n","                bad += 1\n","                if bad >= patience:\n","                    break\n","\n","        model.load_state_dict(best_state)\n","\n","        # final proba\n","        model.eval()\n","        logits_all = []\n","        with torch.no_grad():\n","            for xb, _ in test_loader:\n","                xb = xb.to(device)\n","                logits = model(xb).cpu().numpy()\n","                logits_all.append(logits)\n","        logits_all = np.concatenate(logits_all, axis=0)\n","        proba = softmax_np(logits_all)\n","\n","        all_test_proba[fold_idx-1] = proba\n","        all_test_y[fold_idx-1] = yte\n","\n","        acc, f1m, _ = eval_from_proba(yte, proba)\n","        print(f\"[TS_LSTM_PROBA] Fold {fold_idx} | Acc={acc:.3f} | MacroF1={f1m:.3f}\")\n","\n","    return all_test_proba, all_test_y\n"],"metadata":{"id":"LZt2XArqKHk2","executionInfo":{"status":"ok","timestamp":1768505769836,"user_tz":-180,"elapsed":23,"user":{"displayName":"Hakan Aytuğ Fırat","userId":"02958237446288372335"}}},"execution_count":53,"outputs":[]},{"cell_type":"code","source":["import torch\n","import torch.nn as nn\n","from torch.utils.data import Dataset, DataLoader\n","from transformers import AutoTokenizer, AutoModel\n","from sklearn.metrics import f1_score\n","\n","device = \"cuda\" if torch.cuda.is_available() else \"cpu\"\n","\n","class TextDataset(Dataset):\n","    def __init__(self, texts, y, tokenizer, max_len=192):\n","        self.texts = texts\n","        self.y = y\n","        self.tok = tokenizer\n","        self.max_len = max_len\n","\n","    def __len__(self): return len(self.y)\n","\n","    def __getitem__(self, i):\n","        enc = self.tok(\n","            str(self.texts[i]),\n","            truncation=True,\n","            padding=\"max_length\",\n","            max_length=self.max_len,\n","            return_tensors=\"pt\"\n","        )\n","        item = {k: v.squeeze(0) for k,v in enc.items()}\n","        item[\"y\"] = torch.tensor(int(self.y[i]), dtype=torch.long)\n","        return item\n","\n","class DistilBertCls(nn.Module):\n","    def __init__(self, name=\"distilbert-base-uncased\", n_classes=3, dropout=0.2):\n","        super().__init__()\n","        self.bert = AutoModel.from_pretrained(name)\n","        hid = self.bert.config.hidden_size\n","        self.drop = nn.Dropout(dropout)\n","        self.fc = nn.Linear(hid, n_classes)\n","\n","    def forward(self, input_ids, attention_mask):\n","        out = self.bert(input_ids=input_ids, attention_mask=attention_mask)\n","        cls = out.last_hidden_state[:, 0, :]\n","        return self.fc(self.drop(cls))\n","\n","def train_text_distil_and_proba(df_amzn, folds_amzn, epochs=3, batch=32, lr=2e-5, wd=1e-4, patience=2, max_len=192):\n","    tokenizer = AutoTokenizer.from_pretrained(\"distilbert-base-uncased\")\n","\n","    all_test_proba = [None]*len(folds_amzn)\n","    all_test_y = [None]*len(folds_amzn)\n","\n","    for fold_idx, (tr_i, te_i) in enumerate(folds_amzn, 1):\n","        tr = df_amzn.iloc[tr_i].reset_index(drop=True)\n","        te = df_amzn.iloc[te_i].reset_index(drop=True)\n","\n","        train_loader = DataLoader(\n","            TextDataset(tr[\"text\"].values, tr[\"y\"].values, tokenizer, max_len=max_len),\n","            batch_size=batch, shuffle=True\n","        )\n","        test_loader = DataLoader(\n","            TextDataset(te[\"text\"].values, te[\"y\"].values, tokenizer, max_len=max_len),\n","            batch_size=batch, shuffle=False\n","        )\n","\n","        model = DistilBertCls().to(device)\n","        opt = torch.optim.AdamW(model.parameters(), lr=lr, weight_decay=wd)\n","        loss_fn = nn.CrossEntropyLoss()\n","\n","        best_f1, bad = -1, 0\n","        best_state = None\n","\n","        scaler = torch.cuda.amp.GradScaler(enabled=(device==\"cuda\"))\n","\n","        for ep in range(1, epochs+1):\n","            model.train()\n","            for batch_data in train_loader:\n","                input_ids = batch_data[\"input_ids\"].to(device)\n","                attn = batch_data[\"attention_mask\"].to(device)\n","                yb = batch_data[\"y\"].to(device)\n","\n","                opt.zero_grad(set_to_none=True)\n","                with torch.cuda.amp.autocast(enabled=(device==\"cuda\")):\n","                    logits = model(input_ids, attn)\n","                    loss = loss_fn(logits, yb)\n","\n","                scaler.scale(loss).backward()\n","                scaler.step(opt)\n","                scaler.update()\n","\n","            # eval for early stop\n","            model.eval()\n","            preds, trues = [], []\n","            with torch.no_grad():\n","                for batch_data in test_loader:\n","                    input_ids = batch_data[\"input_ids\"].to(device)\n","                    attn = batch_data[\"attention_mask\"].to(device)\n","                    yb = batch_data[\"y\"].cpu().numpy()\n","                    p = torch.argmax(model(input_ids, attn), dim=1).cpu().numpy()\n","                    preds.extend(p.tolist()); trues.extend(yb.tolist())\n","            f1m = f1_score(trues, preds, average=\"macro\")\n","\n","            if f1m > best_f1 + 1e-4:\n","                best_f1 = f1m\n","                bad = 0\n","                best_state = {k:v.detach().cpu().clone() for k,v in model.state_dict().items()}\n","            else:\n","                bad += 1\n","                if bad >= patience:\n","                    break\n","\n","        model.load_state_dict(best_state)\n","\n","        # final proba\n","        model.eval()\n","        logits_all = []\n","        with torch.no_grad():\n","            for batch_data in test_loader:\n","                input_ids = batch_data[\"input_ids\"].to(device)\n","                attn = batch_data[\"attention_mask\"].to(device)\n","                logits = model(input_ids, attn).cpu().numpy()\n","                logits_all.append(logits)\n","        logits_all = np.concatenate(logits_all, axis=0)\n","        proba = softmax_np(logits_all)\n","\n","        yte = te[\"y\"].values\n","        all_test_proba[fold_idx-1] = proba\n","        all_test_y[fold_idx-1] = yte\n","\n","        acc, f1m, _ = eval_from_proba(yte, proba)\n","        print(f\"[TEXT_DISTIL_PROBA] Fold {fold_idx} | Acc={acc:.3f} | MacroF1={f1m:.3f}\")\n","\n","    return all_test_proba, all_test_y\n"],"metadata":{"id":"wecQHl8XKJqS","executionInfo":{"status":"ok","timestamp":1768505778902,"user_tz":-180,"elapsed":32,"user":{"displayName":"Hakan Aytuğ Fırat","userId":"02958237446288372335"}}},"execution_count":54,"outputs":[]},{"cell_type":"code","source":["# 1) Collect probas\n","p_time_folds, y_time_folds = train_ts_lstm_and_proba(Xw, yw, folds_ts, epochs=20, batch=256)\n","p_text_folds, y_text_folds = train_text_distil_and_proba(df_amzn, folds_amzn, epochs=3, batch=32)\n","\n","# sanity: folds alignment\n","assert len(p_time_folds) == len(p_text_folds) == 5, \"fold sayısı uyuşmuyor\"\n","for k in range(5):\n","    assert len(y_time_folds[k]) == len(y_text_folds[k]), \"fold test size uyuşmuyor\"\n","    assert np.all(np.array(y_time_folds[k]) == np.array(y_text_folds[k])), \"y mismatch! (fold split aynı olmalı)\"\n","\n","# 2) Evaluate fusions\n","avg_accs, avg_f1s = [], []\n","vote_accs, vote_f1s = [], []\n","\n","best_alpha = None\n","best_alpha_score = -1\n","alphas = np.linspace(0.0, 1.0, 11)  # 0..1 step 0.1\n","\n","# alpha select on mean macroF1 across folds (simple)\n","for a in alphas:\n","    f1s = []\n","    for k in range(5):\n","        p = late_fusion_weighted(p_text_folds[k], p_time_folds[k], alpha=float(a))\n","        _, f1m, _ = eval_from_proba(y_time_folds[k], p)\n","        f1s.append(f1m)\n","    mean_f1 = float(np.mean(f1s))\n","    if mean_f1 > best_alpha_score:\n","        best_alpha_score = mean_f1\n","        best_alpha = float(a)\n","\n","print(\"✅ best alpha (time weight):\", best_alpha, \"| mean macroF1:\", best_alpha_score)\n","\n","w_accs, w_f1s = [], []\n","\n","for k in range(5):\n","    ytrue = y_time_folds[k]\n","\n","    p_avg = late_fusion_avg(p_text_folds[k], p_time_folds[k])\n","    acc, f1m, cm = eval_from_proba(ytrue, p_avg)\n","    avg_accs.append(acc); avg_f1s.append(f1m)\n","    print(f\"\\n[LATE_AVG] Fold {k+1} | Acc={acc:.3f} | MacroF1={f1m:.3f}\")\n","    print(cm)\n","\n","    p_w = late_fusion_weighted(p_text_folds[k], p_time_folds[k], alpha=best_alpha)\n","    acc, f1m, cm = eval_from_proba(ytrue, p_w)\n","    w_accs.append(acc); w_f1s.append(f1m)\n","    print(f\"\\n[LATE_WEIGHTED a={best_alpha:.1f}] Fold {k+1} | Acc={acc:.3f} | MacroF1={f1m:.3f}\")\n","    print(cm)\n","\n","    p_vote = late_fusion_vote(p_text_folds[k], p_time_folds[k])\n","    acc, f1m, cm = eval_from_proba(ytrue, p_vote)\n","    vote_accs.append(acc); vote_f1s.append(f1m)\n","    print(f\"\\n[LATE_VOTE] Fold {k+1} | Acc={acc:.3f} | MacroF1={f1m:.3f}\")\n","    print(cm)\n","\n","print(\"\\n=== LATE FUSION SUMMARY ===\")\n","print(\"AVG      | Acc %.3f ± %.3f | F1m %.3f ± %.3f\" % (np.mean(avg_accs), np.std(avg_accs), np.mean(avg_f1s), np.std(avg_f1s)))\n","print(\"WEIGHTED | Acc %.3f ± %.3f | F1m %.3f ± %.3f | alpha=%.1f\" % (np.mean(w_accs), np.std(w_accs), np.mean(w_f1s), np.std(w_f1s), best_alpha))\n","print(\"VOTE     | Acc %.3f ± %.3f | F1m %.3f ± %.3f\" % (np.mean(vote_accs), np.std(vote_accs), np.mean(vote_f1s), np.std(vote_f1s)))\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":1000},"id":"ersXubq9KLQ1","executionInfo":{"status":"error","timestamp":1768505841084,"user_tz":-180,"elapsed":56245,"user":{"displayName":"Hakan Aytuğ Fırat","userId":"02958237446288372335"}},"outputId":"6962d6d8-1e23-4076-a5d0-34c794fc20ee"},"execution_count":55,"outputs":[{"output_type":"stream","name":"stdout","text":["[TS_LSTM_PROBA] Fold 1 | Acc=0.421 | MacroF1=0.399\n","[TS_LSTM_PROBA] Fold 2 | Acc=0.385 | MacroF1=0.352\n","[TS_LSTM_PROBA] Fold 3 | Acc=0.411 | MacroF1=0.388\n","[TS_LSTM_PROBA] Fold 4 | Acc=0.374 | MacroF1=0.300\n","[TS_LSTM_PROBA] Fold 5 | Acc=0.403 | MacroF1=0.365\n"]},{"output_type":"stream","name":"stderr","text":["/tmp/ipython-input-2140457388.py:69: FutureWarning: `torch.cuda.amp.GradScaler(args...)` is deprecated. Please use `torch.amp.GradScaler('cuda', args...)` instead.\n","  scaler = torch.cuda.amp.GradScaler(enabled=(device==\"cuda\"))\n","/tmp/ipython-input-2140457388.py:79: FutureWarning: `torch.cuda.amp.autocast(args...)` is deprecated. Please use `torch.amp.autocast('cuda', args...)` instead.\n","  with torch.cuda.amp.autocast(enabled=(device==\"cuda\")):\n","/tmp/ipython-input-2140457388.py:79: FutureWarning: `torch.cuda.amp.autocast(args...)` is deprecated. Please use `torch.amp.autocast('cuda', args...)` instead.\n","  with torch.cuda.amp.autocast(enabled=(device==\"cuda\")):\n","/tmp/ipython-input-2140457388.py:79: 