"float32\").values\n","y = df_ts[LABEL_COL].astype(\"int64\").values\n","\n","def build_windows(X, y, seq_len):\n","    Xw, yw, idx_end = [], [], []\n","    for i in range(seq_len-1, len(X)):\n","        Xw.append(X[i-seq_len+1:i+1])\n","        yw.append(y[i])          # label at day i\n","        idx_end.append(i)        # end index for mapping folds later\n","    return np.stack(Xw), np.array(yw), np.array(idx_end)\n","\n","Xw, yw, end_idx = build_windows(X_tab, y, SEQ_LEN)\n","\n","print(\"Xw shape:\", Xw.shape)   # (N, 20, 17)\n","print(\"yw shape:\", yw.shape)\n","print(\"end_idx range:\", end_idx.min(), \"->\", end_idx.max())\n","\n","# label dist (windowed)\n","vals, cnts = np.unique(yw, return_counts=True)\n","print(\"window label dist:\", dict(zip(vals, cnts)))\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"ppAi3r1ZIccZ","executionInfo":{"status":"ok","timestamp":1768505333082,"user_tz":-180,"elapsed":22,"user":{"displayName":"Hakan Aytuğ Fırat","userId":"02958237446288372335"}},"outputId":"9ca89548-34bb-4575-8d61-4445a6d751f2"},"execution_count":43,"outputs":[{"output_type":"stream","name":"stdout","text":["Xw shape: (1991, 20, 17)\n","yw shape: (1991,)\n","end_idx range: 19 -> 2009\n","window label dist: {0: 694, 1: 523, 2: 774}\n"]}]},{"cell_type":"code","source":["import numpy as np\n","\n","folds_ts = []\n","for fold, (tr_day, te_day) in enumerate(folds_amzn, 1):\n","    tr_mask = np.isin(end_idx, tr_day)\n","    te_mask = np.isin(end_idx, te_day)\n","\n","    tr_i = np.where(tr_mask)[0]\n","    te_i = np.where(te_mask)[0]\n","\n","    folds_ts.append((tr_i, te_i))\n","\n","    tr_dist = np.bincount(yw[tr_i], minlength=3)\n","    te_dist = np.bincount(yw[te_i], minlength=3)\n","    print(f\"Fold {fold}: train={len(tr_i)} val/test={len(te_i)} | train_dist={tr_dist} | test_dist={te_dist}\")\n","\n","print(\"✅ folds_ts ready:\", len(folds_ts))\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"rAS1FMD9IhtJ","executionInfo":{"status":"ok","timestamp":1768505353908,"user_tz":-180,"elapsed":18,"user":{"displayName":"Hakan Aytuğ Fırat","userId":"02958237446288372335"}},"outputId":"d9abee7d-feda-40fc-cc8a-c04174e55e2e"},"execution_count":44,"outputs":[{"output_type":"stream","name":"stdout","text":["Fold 1: train=1592 val/test=399 | train_dist=[555 418 619] | test_dist=[139 105 155]\n","Fold 2: train=1596 val/test=395 | train_dist=[555 419 622] | test_dist=[139 104 152]\n","Fold 3: train=1592 val/test=399 | train_dist=[556 418 618] | test_dist=[138 105 156]\n","Fold 4: train=1593 val/test=398 | train_dist=[555 419 619] | test_dist=[139 104 155]\n","Fold 5: train=1591 val/test=400 | train_dist=[555 418 618] | test_dist=[139 105 156]\n","✅ folds_ts ready: 5\n"]}]},{"cell_type":"code","source":["import torch\n","import torch.nn as nn\n","from torch.utils.data import Dataset, DataLoader\n","from sklearn.preprocessing import StandardScaler\n","from sklearn.metrics import accuracy_score, f1_score, confusion_matrix\n","\n","device = \"cuda\" if torch.cuda.is_available() else \"cpu\"\n","print(\"device:\", device, \"| gpu:\", torch.cuda.get_device_name(0) if device==\"cuda\" else None)\n","\n","class SeqDataset(Dataset):\n","    def __init__(self, Xw, y):\n","        self.X = torch.from_numpy(Xw)       # (N, T, F)\n","        self.y = torch.from_numpy(y)\n","    def __len__(self): return len(self.y)\n","    def __getitem__(self, i):\n","        return self.X[i], self.y[i]\n","\n","class LSTMClassifier(nn.Module):\n","    def __init__(self, n_feat, hidden=128, n_layers=2, dropout=0.2, n_classes=3):\n","        super().__init__()\n","        self.lstm = nn.LSTM(\n","            input_size=n_feat,\n","            hidden_size=hidden,\n","            num_layers=n_layers,\n","            batch_first=True,\n","            dropout=dropout if n_layers > 1 else 0.0,\n","            bidirectional=False\n","        )\n","        self.drop = nn.Dropout(dropout)\n","        self.fc = nn.Linear(hidden, n_classes)\n","\n","    def forward(self, x):\n","        out, (hn, cn) = self.lstm(x)     # hn: (layers, B, hidden)\n","        last = hn[-1]                    # (B, hidden)\n","        return self.fc(self.drop(last))\n","\n","def metrics(y_true, y_pred):\n","    return accuracy_score(y_true, y_pred), f1_score(y_true, y_pred, average=\"macro\")\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"1vHL5Hp_Iktf","executionInfo":{"status":"ok","timestamp":1768505365415,"user_tz":-180,"elapsed":27,"user":{"displayName":"Hakan Aytuğ Fırat","userId":"02958237446288372335"}},"outputId":"9e1845bd-7d50-4d58-d42b-658c3d46774b"},"execution_count":45,"outputs":[{"output_type":"stream","name":"stdout","text":["device: cuda | gpu: NVIDIA A100-SXM4-80GB\n"]}]},{"cell_type":"code","source":["import numpy as np\n","import torch\n","import torch.nn as nn\n","from torch.utils.data import Dataset, DataLoader\n","from sklearn.preprocessing import StandardScaler\n","from sklearn.metrics import accuracy_score, f1_score, confusion_matrix\n","\n","device = \"cuda\" if torch.cuda.is_available() else \"cpu\"\n","\n","class SeqDataset(Dataset):\n","    def __init__(self, Xw, y):\n","        self.X = torch.from_numpy(Xw)  # (N,T,F)\n","        self.y = torch.from_numpy(y)\n","    def __len__(self): return len(self.y)\n","    def __getitem__(self, i):\n","        return self.X[i], self.y[i]\n","\n","class LSTMClassifier(nn.Module):\n","    def __init__(self, n_feat, hidden=128, n_layers=2, dropout=0.2, n_classes=3):\n","        super().__init__()\n","        self.lstm = nn.LSTM(\n","            input_size=n_feat,\n","            hidden_size=hidden,\n","            num_layers=n_layers,\n","            batch_first=True,\n","            dropout=dropout if n_layers > 1 else 0.0,\n","            bidirectional=False\n","        )\n","        self.drop = nn.Dropout(dropout)\n","        self.fc = nn.Linear(hidden, n_classes)\n","\n","    def forward(self, x):\n","        _, (hn, _) = self.lstm(x)\n","        last = hn[-1]  # (B, hidden)\n","        return self.fc(self.drop(last))\n","\n","def metrics(y_true, y_pred):\n","    return accuracy_score(y_true, y_pred), f1_score(y_true, y_pred, average=\"macro\")\n","\n","EPOCHS = 30\n","BATCH = 256\n","LR = 1e-3\n","WD = 1e-4\n","PATIENCE = 5\n","\n","lstm_accs, lstm_f1s = [], []\n","\n","for fold_idx, (tr_i, te_i) in enumerate(folds_ts, 1):\n","    Xtr, Xte = Xw[tr_i].copy(), Xw[te_i].copy()\n","    ytr, yte = yw[tr_i].copy(), yw[te_i].copy()\n","\n","    # --- scale (fit train only) ---\n","    scaler = StandardScaler()\n","    Xtr2 = Xtr.reshape(-1, Xtr.shape[-1])\n","    Xte2 = Xte.reshape(-1, Xte.shape[-1])\n","    Xtr2 = scaler.fit_transform(Xtr2).astype(\"float32\")\n","    Xte2 = scaler.transform(Xte2).astype(\"float32\")\n","    Xtr = Xtr2.reshape(Xtr.shape)\n","    Xte = Xte2.reshape(Xte.shape)\n","\n","    train_loader = DataLoader(SeqDataset(Xtr, ytr), batch_size=BATCH, shuffle=True)\n","    test_loader  = DataLoader(SeqDataset(Xte, yte), batch_size=BATCH, shuffle=False)\n","\n","    model = LSTMClassifier(n_feat=Xtr.shape[-1]).to(device)\n","    opt = torch.optim.AdamW(model.parameters(), lr=LR, weight_decay=WD)\n","    loss_fn = nn.CrossEntropyLoss()\n","\n","    best_f1, patience = -1, 0\n","    best_state = None\n","\n","    for ep in range(1, EPOCHS+1):\n","        model.train()\n","        total_loss = 0.0\n","        for xb, yb in train_loader:\n","            xb, yb = xb.to(device), yb.to(device)\n","            opt.zero_grad(set_to_none=True)\n","            logits = model(xb)\n","            loss = loss_fn(logits, yb)\n","            loss.backward()\n","            opt.step()\n","            total_loss += loss.item() * len(yb)\n","\n","        model.eval()\n","        preds, trues = [], []\n","        with torch.no_grad():\n","            for xb, yb in test_loader:\n","                xb = xb.to(device)\n","                p = torch.argmax(model(xb), dim=1).cpu().numpy()\n","                preds.extend(p.tolist())\n","                trues.extend(yb.numpy().tolist())\n","\n","        acc, f1m = metrics(trues, preds)\n","\n","        if ep == 1 or ep % 2 == 0:\n","            print(f\"[TS_LSTM] Fold {fold_idx} | ep {ep:02d}/{EPOCHS} | loss={total_loss/len(tr_i):.4f} | acc={acc:.3f} | macroF1={f1m:.3f}\")\n","\n","        if f1m > best_f1 + 1e-4:\n","            best_f1 = f1m\n","            patience = 0\n","            best_state = {k:v.detach().cpu().clone() for k,v in model.state_dict().items()}\n","        else:\n","            patience += 1\n","            if patience >= PATIENCE:\n","                break\n","\n","    model.load_state_dict(best_state)\n","\n","    # final\n","    model.eval()\n","    preds, trues = [], []\n","    with torch.no_grad():\n","        for xb, yb in test_loader:\n","            xb = xb.to(device)\n","            p = torch.argmax(model(xb), dim=1).cpu().numpy()\n","            preds.extend(p.tolist())\n","            trues.extend(yb.numpy().tolist())\n","\n","    acc, f1m = metrics(trues, preds)\n","    lstm_accs.append(acc); lstm_f1s.append(f1m)\n","\n","    print(f\"\\n[TS_LSTM] Fold {fold_idx} FINAL | Acc={acc:.3f} | MacroF1={f1m:.3f}\")\n","    print(confusion_matrix(trues, preds))\n","\n","print(\"\\n=== TS_LSTM SUMMARY ===\")\n","print(\"Acc : %.3f ± %.3f\" % (np.mean(lstm_accs), np.std(lstm_accs)))\n","print(\"F1m : %.3f ± %.3f\" % (np.mean(lstm_f1s), np.std(lstm_f1s)))\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"1tD3LLcCI1I-","executionInfo":{"status":"ok","timestamp":1768505437608,"user_tz":-180,"elapsed":4743,"user":{"displayName":"Hakan Aytuğ Fırat","userId":"02958237446288372335"}},"outputId":"185a8127-8274-41f5-a144-a087fda18356"},"execution_count":46,"outputs":[{"output_type":"stream","name":"stdout","text":["[TS_LSTM] Fold 1 | ep 01/30 | loss=1.0899 | acc=0.391 | macroF1=0.246\n","[TS_LSTM] Fold 1 | ep 02/30 | loss=1.0811 | acc=0.396 | macroF1=0.236\n","[TS_LSTM] Fold 1 