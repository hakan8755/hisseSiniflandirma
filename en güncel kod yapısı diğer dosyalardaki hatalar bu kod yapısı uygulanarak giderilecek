port numpy as np\n","import torch\n","import torch.nn as nn\n","from torch.utils.data import Dataset, DataLoader\n","\n","from sklearn.metrics import accuracy_score, f1_score, classification_report, confusion_matrix\n","from sklearn.preprocessing import StandardScaler\n","\n","def seed_all(seed=42):\n","    import random, os\n","    random.seed(seed)\n","    np.random.seed(seed)\n","    torch.manual_seed(seed)\n","    torch.cuda.manual_seed_all(seed)\n","\n","seed_all(42)\n","\n","device = \"cuda\" if torch.cuda.is_available() else \"cpu\"\n","print(\"device:\", device, \"| gpu:\", torch.cuda.get_device_name(0) if device==\"cuda\" else None)\n","\n","def compute_metrics(y_true, y_pred):\n","    acc = accuracy_score(y_true, y_pred)\n","    f1m = f1_score(y_true, y_pred, average=\"macro\")\n","    return acc, f1m\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"C-kPLSmfBCtZ","executionInfo":{"status":"ok","timestamp":1768503392726,"user_tz":-180,"elapsed":19,"user":{"displayName":"Hakan Aytuğ Fırat","userId":"02958237446288372335"}},"outputId":"bf57e125-d1c1-4d75-8f90-cbd9f8f6533f"},"execution_count":21,"outputs":[{"output_type":"stream","name":"stdout","text":["device: cuda | gpu: NVIDIA A100-SXM4-80GB\n"]}]},{"cell_type":"code","source":["# Tabular feature listesi\n","TAB_COLS = feat_cols  # önceki hücreden geliyor\n","print(\"TAB_COLS:\", len(TAB_COLS))\n","\n","X_tab = df[TAB_COLS].astype(\"float32\").values\n","y = df[\"y\"].values.astype(\"int64\")\n","\n","class TabDataset(Dataset):\n","    def __init__(self, X, y):\n","        self.X = torch.from_numpy(X)\n","        self.y = torch.from_numpy(y)\n","    def __len__(self): return len(self.y)\n","    def __getitem__(self, i):\n","        return self.X[i], self.y[i]\n","\n","class MLP(nn.Module):\n","    def __init__(self, in_dim, n_classes=3):\n","        super().__init__()\n","        self.net = nn.Sequential(\n","            nn.Linear(in_dim, 256),\n","            nn.ReLU(),\n","            nn.Dropout(0.2),\n","            nn.Linear(256, 128),\n","            nn.ReLU(),\n","            nn.Dropout(0.2),\n","            nn.Linear(128, n_classes),\n","        )\n","    def forward(self, x):\n","        return self.net(x)\n","\n","print(\"X_tab shape:\", X_tab.shape, \"| y shape:\", y.shape)\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"3JqtNToABEkj","executionInfo":{"status":"ok","timestamp":1768503398282,"user_tz":-180,"elapsed":25,"user":{"displayName":"Hakan Aytuğ Fırat","userId":"02958237446288372335"}},"outputId":"47379b77-5172-4b54-fc83-4b8b90a84685"},"execution_count":22,"outputs":[{"output_type":"stream","name":"stdout","text":["TAB_COLS: 17\n","X_tab shape: (2010, 17) | y shape: (2010,)\n"]}]},{"cell_type":"code","source":["from tqdm.auto import tqdm\n","\n","EPOCHS = 20\n","BATCH = 128\n","LR = 1e-3\n","WD = 1e-4\n","\n","tab_accs, tab_f1s = [], []\n","\n","for fold_idx, (tr_idx, te_idx) in enumerate(folds, 1):\n","    # split\n","    Xtr, Xte = X_tab[tr_idx], X_tab[te_idx]\n","    ytr, yte = y[tr_idx], y[te_idx]\n","\n","    # scale (train fit, test transform) - leakage yok\n","    scaler = StandardScaler()\n","    Xtr = scaler.fit_transform(Xtr).astype(\"float32\")\n","    Xte = scaler.transform(Xte).astype(\"float32\")\n","\n","    train_loader = DataLoader(TabDataset(Xtr, ytr), batch_size=BATCH, shuffle=True, drop_last=False)\n","    test_loader  = DataLoader(TabDataset(Xte, yte), batch_size=BATCH, shuffle=False, drop_last=False)\n","\n","    model = MLP(in_dim=Xtr.shape[1], n_classes=3).to(device)\n","    opt = torch.optim.AdamW(model.parameters(), lr=LR, weight_decay=WD)\n","    loss_fn = nn.CrossEntropyLoss()\n","\n","    best_f1 = -1\n","    patience, PATIENCE = 0, 4\n","\n","    for ep in range(1, EPOCHS+1):\n","        model.train()\n","        total_loss = 0.0\n","        for xb, yb in train_loader:\n","            xb, yb = xb.to(device), yb.to(device)\n","            opt.zero_grad(set_to_none=True)\n","            logits = model(xb)\n","            loss = loss_fn(logits, yb)\n","            loss.backward()\n","            opt.step()\n","            total_loss += loss.item() * len(yb)\n","\n","        # eval\n","        model.eval()\n","        preds, trues = [], []\n","        with torch.no_grad():\n","            for xb, yb in test_loader:\n","                xb = xb.to(device)\n","                logits = model(xb)\n","                p = torch.argmax(logits, dim=1).cpu().numpy()\n","                preds.extend(p.tolist())\n","                trues.extend(yb.numpy().tolist())\n","\n","        acc, f1m = compute_metrics(trues, preds)\n","\n","        if f1m > best_f1 + 1e-4:\n","            best_f1 = f1m\n","            patience = 0\n","            best_state = {k:v.detach().cpu().clone() for k,v in model.state_dict().items()}\n","        else:\n","            patience += 1\n","\n","        if ep % 2 == 0 or ep == 1:\n","            print(f\"[TAB_MLP] Fold {fold_idx} | ep {ep:02d}/{EPOCHS} | loss={total_loss/len(tr_idx):.4f} | acc={acc:.3f} | macroF1={f1m:.3f}\")\n","\n","        if patience >= PATIENCE:\n","            break\n","\n","    # load best\n","    model.load_state_dict(best_state)\n","\n","    # final eval (best state)\n","    model.eval()\n","    preds, trues = [], []\n","    with torch.no_grad():\n","        for xb, yb in test_loader:\n","            xb = xb.to(device)\n","            logits = model(xb)\n","            p = torch.argmax(logits, dim=1).cpu().numpy()\n","            preds.extend(p.tolist())\n","            trues.extend(yb.numpy().tolist())\n","\n","    acc, f1m = compute_metrics(trues, preds)\n","    tab_accs.append(acc); tab_f1s.append(f1m)\n","\n","    print(f\"\\n[TAB_MLP] Fold {fold_idx} FINAL | Acc={acc:.3f} | MacroF1={f1m:.3f}\")\n","    print(confusion_matrix(trues, preds))\n","\n","print(\"\\n=== TAB_MLP SUMMARY ===\")\n","print(\"Acc : %.3f ± %.3f\" % (np.mean(tab_accs), np.std(tab_accs)))\n","print(\"F1m : %.3f ± %.3f\" % (np.mean(tab_f1s), np.std(tab_f1s)))\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"ZjvGUskWBF1h","executionInfo":{"status":"ok","timestamp":1768503411726,"user_tz":-180,"elapsed":8170,"user":{"displayName":"Hakan Aytuğ Fırat","userId":"02958237446288372335"}},"outputId":"d40f9a4d-a33a-4fd3-e58d-589e2d043297"},"execution_count":23,"outputs":[{"output_type":"stream","name":"stdout","text":["[TAB_MLP] Fold 1 | ep 01/20 | loss=1.0108 | acc=0.483 | macroF1=0.367\n","[TAB_MLP] Fold 1 | ep 02/20 | loss=0.9575 | acc=0.485 | macroF1=0.375\n","[TAB_MLP] Fold 1 | ep 04/20 | loss=0.9414 | acc=0.498 | macroF1=0.379\n","[TAB_MLP] Fold 1 | ep 06/20 | loss=0.9323 | acc=0.493 | macroF1=0.375\n","[TAB_MLP] Fold 1 | ep 08/20 | loss=0.9301 | acc=0.505 | macroF1=0.385\n","[TAB_MLP] Fold 1 | ep 10/20 | loss=0.9261 | acc=0.517 | macroF1=0.395\n","[TAB_MLP] Fold 1 | ep 12/20 | loss=0.9192 | acc=0.517 | macroF1=0.394\n","[TAB_MLP] Fold 1 | ep 14/20 | loss=0.9159 | acc=0.515 | macroF1=0.393\n","\n","[TAB_MLP] Fold 1 FINAL | Acc=0.520 | MacroF1=0.402\n","[[103   1  46]\n"," [ 50   1  49]\n"," [ 47   0 105]]\n","[TAB_MLP] Fold 2 | ep 01/20 | loss=1.0132 | acc=0.540 | macroF1=0.411\n","[TAB_MLP] Fold 2 | ep 02/20 | loss=0.9626 | acc=0.527 | macroF1=0.403\n","[TAB_MLP] Fold 2 | ep 04/20 | loss=0.9461 | acc=0.532 | macroF1=0.406\n","\n","[TAB_MLP] Fold 2 FINAL | Acc=0.540 | MacroF1=0.411\n","[[ 98   0  52]\n"," [ 46   0  55]\n"," [ 32   0 119]]\n","[TAB_MLP] Fold 3 | ep 01/20 | loss=1.0045 | acc=0.500 | macroF1=0.380\n","[TAB_MLP] Fold 3 | ep 02/20 | loss=0.9633 | acc=0.495 | macroF1=0.400\n","[TAB_MLP] Fold 3 | ep 04/20 | loss=0.9441 | acc=0.512 | macroF1=0.391\n","[TAB_MLP] Fold 3 | ep 06/20 | loss=0.9402 | acc=0.500 | macroF1=0.381\n","\n","[TAB_MLP] Fold 3 FINAL | Acc=0.495 | MacroF1=0.400\n","[[ 90   5  55]\n"," [ 57   4  40]\n"," [ 42   4 105]]\n","[TAB_MLP] Fold 4 | ep 01/20 | loss=1.0207 | acc=0.537 | macroF1=0.409\n","[TAB_MLP] Fold 4 | ep 02/20 | loss=0.9584 | acc=0.542 | m