\n"," [25 97 34]]\n","\n","=== INTERMEDIATE FUSION V2 SUMMARY ===\n","Acc : 0.354 ± 0.018\n","F1m : 0.327 ± 0.019\n"]}]},{"cell_type":"code","source":["import re\n","import pandas as pd\n","import matplotlib.pyplot as plt\n","import seaborn as sns\n","import numpy as np\n","\n","# =============================================================================\n","# 1. ADIM: EĞİTİM SONUÇLARINI AŞAĞIDAKİ TIRNAKLARIN ARASINA YAPIŞTIR\n","# =============================================================================\n","log_verisi = \"\"\"\n","[INTER_V2] Fold 1 FINAL | Acc=0.376 | MacroF1=0.296\n","[INTER_V2] Fold 2 FINAL | Acc=0.370 | MacroF1=0.346\n","[INTER_V2] Fold 3 FINAL | Acc=0.358 | MacroF1=0.345\n","[INTER_V2] Fold 4 FINAL | Acc=0.339 | MacroF1=0.333\n","[INTER_V2] Fold 5 FINAL | Acc=0.328 | MacroF1=0.317\n","[[64 46 29]\n"," [47 35 22]\n"," [56 63 36]]\n","\"\"\"\n","# (Yukarıdaki örnek verileri silip kendi çıktılarını yapıştırabilirsin)\n","# =============================================================================\n","\n","# 2. ADIM: VERİLERİ AYIKLA\n","training_data = []\n","final_results = []\n","confusion_matrices = []\n","\n","# Regex Desenleri\n","log_pattern = re.compile(r'\\[(.*?)\\] Fold (\\d+) \\| ep (\\d+)/(\\d+) \\| Acc=([\\d\\.]+) \\| MacroF1=([\\d\\.]+)')\n","final_pattern = re.compile(r'\\[(.*?)\\] Fold (\\d+) FINAL \\| Acc=([\\d\\.]+) \\| MacroF1=([\\d\\.]+)')\n","\n","lines = log_verisi.strip().split('\\n')\n","cm_buffer = []\n","capturing_cm = False\n","current_cm_meta = {}\n","\n","for line in lines:\n","    line = line.strip()\n","\n","    # A) Eğitim Loglarını Yakala\n","    match_log = log_pattern.match(line)\n","    if match_log:\n","        model, fold, epoch, total, acc, f1 = match_log.groups()\n","        training_data.append({'Model': model, 'Fold': int(fold), 'Epoch': int(epoch), 'Accuracy': float(acc), 'MacroF1': float(f1)})\n","        continue\n","\n","    # B) Final Sonuçlarını Yakala\n","    match_final = final_pattern.match(line)\n","    if match_final:\n","        model, fold, acc, f1 = match_final.groups()\n","        final_results.append({'Model': model, 'Fold': int(fold), 'Final_Accuracy': float(acc), 'Final_MacroF1': float(f1)})\n","        capturing_cm = True\n","        current_cm_meta = {'Model': model, 'Fold': int(fold)}\n","        cm_buffer = []\n","        continue\n","\n","    # C) Matris Yakala\n","    if capturing_cm:\n","        if line.startswith('[') and line.endswith(']'):\n","            temp = line.replace('[', '').replace(']', '').strip()\n","            try:\n","                row = [int(x) for x in temp.split()]\n","                cm_buffer.append(row)\n","            except: pass\n","            if line.endswith(']]'):\n","                confusion_matrices.append({'Model': current_cm_meta['Model'], 'Matrix': cm_buffer})\n","                capturing_cm = False\n","        elif line == '': pass\n","        else: capturing_cm = False\n","\n","# DataFrame oluştur\n","df_final = pd.DataFrame(final_results)\n","df_train = pd.DataFrame(training_data)\n","\n","# 3. ADIM: GÖRSELLEŞTİRME\n","sns.set(style=\"whitegrid\")\n","\n","# --- Grafik 1: Performans Karşılaştırması ---\n","if not df_final.empty:\n","    plt.figure(figsize=(12, 6))\n","    df_melted = df_final.melt(id_vars=['Model', 'Fold'], value_vars=['Final_Accuracy', 'Final_MacroF1'], var_name='Metric', value_name='Score')\n","    sns.barplot(data=df_melted, x='Model', y='Score', hue='Metric', errorbar='sd', palette='viridis')\n","    plt.title('Modellerin Başarısı (Accuracy & F1)')\n","    plt.xticks(rotation=45, ha='right')\n","    plt.tight_layout()\n","    plt.show()\n","else:\n","    print(\"⚠️ 'FINAL' etiketli sonuç bulunamadı. Lütfen çıktıları doğru yapıştırdığından emin ol.\")\n","\n","# --- Grafik 2: Eğitim Eğrileri ---\n","if not df_train.empty:\n","    train_agg = df_train.groupby(['Model', 'Epoch'])[['Accuracy', 'MacroF1']].mean().reset_index()\n","    fig, axes = plt.subplots(1, 2, figsize=(16, 5))\n","    sns.lineplot(data=train_agg, x='Epoch', y='Accuracy', hue='Model', ax=axes[0], marker='o')\n","    axes[0].set_title('Eğitim Süreci (Accuracy)')\n","    sns.lineplot(data=train_agg, x='Epoch', y='MacroF1', hue='Model', ax=axes[1], marker='o')\n","    axes[1].set_title('Eğitim Süreci (Macro F1)')\n","    plt.show()\n","\n","# --- Grafik 3: Karmaşıklık Matrisleri ---\n","aggregated_cms = {}\n","for entry in confusion_matrices:\n","    model = entry['Model']\n","    if model not in aggregated_cms: aggregated_cms[model] = np.array(entry['Matrix'])\n","    else: aggregated_cms[model] += np.array(entry['Matrix'])\n","\n","if aggregated_cms:\n","    n = len(aggregated_cms)\n","    cols = 3\n","    rows = (n + cols - 1) // cols\n","    fig, axes = plt.subplots(rows, cols, figsize=(15, 4 * rows))\n","    if n == 1: axes = [axes]\n","    axes = np.array(axes).flatten()\n","    for i, (m, cm) in enumerate(aggregated_cms.items()):\n","        sns.heatmap(cm, annot=True, fmt='d', cmap='Blues', ax=axes[i], cbar=False)\n","        axes[i].set_title(m)\n","    for j in range(i+1, len(axes)): axes[j].axis('off')\n","    plt.tight_layout()\n","    plt.show()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":646},"id":"d44NiD9eP8XK","executionInfo":{"status":"ok","timestamp":1768508107288,"user_tz":-180,"elapsed":840,"user":{"displayName":"Hakan Aytuğ Fırat","userId":"02958237446288372335"}},"outputId":"8205ff55-0076-4a86-a7b4-b6625f334b6d"},"execution_count":78,"outputs":[{"output_type":"display_data","data":{"text/plain":["<Figure size 1200x600 with 1 Axes>"],"image/png":"\n"},"metadata":{}},{"output_type":"display_data","data":{"text/plain":["<Figure size 1500x400 with 3 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"code","source":["import numpy as np\n","import matplotlib.pyplot as plt\n","\n","# -----------------------------\n","# 1) Helper: mean±std bar plot\n","# -----------------------------\n","def _mean_std(x):\n","    x = np.array(x, dtype=float)\n","    return float(x.mean()), float(x.std())\n","\n","def plot_summary_bars(results, title=\"Model Performance Summary\", metric_order=(\"Acc\", \"MacroF1\")):\n","    \"\"\"\n","    results: dict\n","      ör: {\n","        \"TS_LSTM\": {\"Acc\": [..5..], \"MacroF1\":[..5..]},\n","        \"TEXT_DISTIL\": {\"Acc\":[..], \"MacroF1\":[..]},\n","        ...\n","      }\n","    \"\"\"\n","    # filtrele (boş olanları alma)\n","    filtered = {}\n","    for name, d in results.items():\n","        if not isinstance(d, dict):\n","            continue\n","        ok = True\n","        for m in metric_order:\n","            if m not in d or d[m] is None or len(d[m]) == 0:\n","                ok = False\n","        if ok:\n","            filtered[name] = d\n","\n","    if len(filtered) == 0:\n","        print(\"⚠️ plot_summary_bars: Çizecek veri bulamadım. results dict'ini kontrol et.\")\n","        return\n","\n","    names = list(filtered.keys())\n","    x = np.arange(len(names))\n","    width = 0.38\n","\n","    fig, ax = plt.subplots(figsize=(max(10, len(names)*1.2), 5))\n","\n","    for j, metric in enumerate(metric_order):\n","        means = []\n","        stds  = []\n","        for n in names:\n","            mu, sd = _mean_std(filtered[n][metric])\n","            means.append(mu); stds.append(sd)\n","        offset = (-width/2) if j == 0 else (width/2)\n","        ax.bar(x + offset, means, width, yerr=stds, capsize=4, label=f\"{metric} (mean±std)\")\n","\n","    ax.set_xticks(x)\n","    ax.set_xticklabels(names, rotation=25, ha=\"right\")\n","    ax.set_ylim(0, 1.0)\n","    ax.set_ylabel(\"Score\")\n","    ax.set_title(title)\n","    ax.legend()\n","    ax.grid(True, axis=\"y\", linestyle=\"--\", alpha=0.4)\n","    plt.tight_layout()\n","    plt.show()\n","\n","\n","# -----------------------------\n","# 2) Helper: Confusion Matrix\n","# -----------------------------\n","def plot_confusion(cm, labels=(\"DOWN\",\"FLAT\",\"UP\"), title=\"Confusion Matrix\", normalize=False):\n","    cm = np.array(cm, dtype=float)\n","    if normalize:\n","        row_sums = cm.sum(axis=1, keepdims=True)\n","        row_sums[row_sums == 0] = 1.0\n","        cm = cm / row_sums\n","\n","    fig, ax = plt.subplots(figsize=(5, 4))\n","    im = ax.imshow(cm, interpolation=\"nearest\")\n","    ax.set_title(title)\n","    ax.set_xlabel(\"Predicted\")\n","    ax.set_ylabel(\"True\")\n","    ax.set_xticks(np.arange(len(labels)))\n","    ax.set_yticks(np.arange(len(labels)))\n","    ax.set_xticklabels(labels)\n","    ax.set_yticklabels(labels)\n","\n","    # hücre yazıları\n","    for i in range(cm.shape[0]):\n","        for j in range(cm.shape[1]):\n","            val = cm[i, j]\n","            txt = f\"{val:.2f}\" if normalize else f\"{int(val)}\"\n","            ax.text(j, i, txt, ha=\"center\", va=\"center\")\n","\n","    plt.tight_layout()\n","    plt.show()\n","\n","\n","# -------------------------------------------------\n","# 3) AUTO-COLLECT: elinde varsa yakala ve çiz\n","# -------------------------------------------------\n","# Buraya, senin kodunda oluşan listeleri otomatik toplamaya çalışıyorum.\n","# Yoksa sorun değil, sadece olanları çizer.\n","\n","results = {}\n","\n","def try_add(name, acc_list_name, f1_list_name):\n","    g = globals()\n","    if acc_list_name in g and f1_list_name in g:\n","        acc = g[acc_list_name]\n","        f1  = g[f1_list_name]\n","        if acc is not None and f1 is not None and len(acc) > 0 and len(f1) > 0:\n","            results[name] = {\"Acc\": list(acc), \"MacroF1\": list(f1)}\n","\n","# Sende olabilecek yaygın isimler:\n","try_add(\"TS_LSTM\", \"lstm_accs\", \"lstm_f1s\")                 # TS_LSTM SUMMARY'deki listeler\n","try_add(\"EARLY_MLP\", \"early_accs\", \"early_f1s\")             # varsa\n","try_add(\"EARLY_MLP\", \"emlp_accs\", \"emlp_f1s\")               # bazen böyle\n","try_add(\"INTER_MLP\", \"inter_accs\", \"inter_f1s\")             # varsa\n","try_add(\"INTER_MLP\", \"imlp_accs\", \"imlp_f1s\")               # bazen böyle\n","try_add(\"TEXT_DISTIL\", \"text_accs\", \"text_f1s\")             # varsa\n","try_add(\"TEXT_DISTIL\", \"td_accs\", \"td_f1s\")                 # bazen böyle\n","\n","# Late fusion için genelde tek summary var ama fold fold sakladıysan:\n","try_add(\"LATE_AVG\", \"late_avg_accs\", \"late_avg_f1s\")\n","try_add(\"LATE_WEIGHTED\", \"late_w_accs\", \"late_w_f1s\")\n","try_add(\"LATE_VOTE\", \"late_vote_accs\", \"late_vote_f1s\")\n","\n","# Eğer sen sadece mean±std yazdırdıysan ve liste yoksa:\n","# -> aşağıdaki bölüme MANUEL ekleyebilirsin (örnek format verdim)\n","\n","# ÖRNEK MANUEL EKLEME:\n","# results[\"LATE_WEIGHTED\"] = {\"Acc\":[0.405,0.405,0.411,0.392,0.427], \"MacroF1\":[0.408,0.338,0.359,0.346,0.368]}\n","\n","print(\"📌 Bulunan result setleri:\", list(results.keys()))\n","\n","plot_summary_bars(results, title=\"AMZN | 5-Fold Model Summary (mean±std)\")\n","\n","\n","# -------------------------------------------------\n","# 4) (Opsiyonel) Confusion matrix çizimi\n","# -------------------------------------------------\n","# Elinde tek bir cm varsa:\n","# plot_confusion(cm, labels=(\"DOWN\",\"FLAT\",\"UP\"), title=\"TS_LSTM Fold-1 CM\", normalize=False)\n","\n","# Eğer fold fold cm sakladıysan (ör: cms_ts_lstm = [cm1,cm2,...]):\n","if \"cms_ts_lstm\" in globals() and isinstance(globals()[\"cms_ts_lstm\"], (list, tuple)) and len(globals()[\"cms_ts_lstm\"]) > 0:\n","    for i, cm in enumerate(globals()[\"cms_ts_lstm\"], 1):\n","        plot_confusion(cm, labels=(\"DOWN\",\"FLAT\",\"UP\"), title=f\"TS_LSTM Fold {i} CM\", normalize=False)\n"],"metadata":{"id":"hGdUeJVFTFZ0","executionInfo":{"status":"ok","timestamp":1768508124321,"user_tz":-180,"elapsed":385,"user":{"displayName":"Hakan Aytuğ Fırat","userId":"02958237446288372335"}},"outputId":"5d586324-01b9-41bc-8674-f9018c97047e","colab":{"base_uri":"https://localhost:8080/","height":446}},"execution_count":79,"outputs":[{"output_type":"stream","name":"std