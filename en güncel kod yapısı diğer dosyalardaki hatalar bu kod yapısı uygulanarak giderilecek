","userId":"02958237446288372335"}},"outputId":"3f0464a9-d7ec-47f5-929a-da2c50557f3c"},"execution_count":24,"outputs":[{"output_type":"stream","name":"stdout","text":["Vocab size: 14716\n","Sample vocab: [('<PAD>', 0), ('<UNK>', 1), ('the', 2), ('to', 3), ('it', 4), ('i', 5), ('is', 6), ('a', 7), ('and', 8), ('you', 9)]\n"]}]},{"cell_type":"code","source":["class TextDataset(Dataset):\n","    def __init__(self, texts, y, word2idx, max_len=200):\n","        self.texts = texts\n","        self.y = y\n","        self.word2idx = word2idx\n","        self.max_len = max_len\n","\n","    def encode(self, text):\n","        tokens = basic_tokenize(text)\n","        ids = [self.word2idx.get(t, 1) for t in tokens][:self.max_len]\n","        if len(ids) < self.max_len:\n","            ids += [0] * (self.max_len - len(ids))\n","        return torch.tensor(ids, dtype=torch.long)\n","\n","    def __len__(self):\n","        return len(self.y)\n","\n","    def __getitem__(self, i):\n","        return self.encode(self.texts[i]), torch.tensor(self.y[i])\n"],"metadata":{"id":"ZOxCiXp_BUuf","executionInfo":{"status":"ok","timestamp":1768503464614,"user_tz":-180,"elapsed":45,"user":{"displayName":"Hakan Aytuğ Fırat","userId":"02958237446288372335"}}},"execution_count":25,"outputs":[]},{"cell_type":"code","source":["class BiLSTM(nn.Module):\n","    def __init__(self, vocab_size, emb_dim=128, hid_dim=128, n_classes=3):\n","        super().__init__()\n","        self.embedding = nn.Embedding(vocab_size, emb_dim, padding_idx=0)\n","        self.lstm = nn.LSTM(\n","            emb_dim,\n","            hid_dim,\n","            batch_first=True,\n","            bidirectional=True\n","        )\n","        self.fc = nn.Linear(hid_dim * 2, n_classes)\n","\n","    def forward(self, x):\n","        emb = self.embedding(x)\n","        out, (h, _) = self.lstm(emb)\n","        h_cat = torch.cat((h[-2], h[-1]), dim=1)\n","        return self.fc(h_cat)\n"],"metadata":{"id":"epyXyVPzBWVI","executionInfo":{"status":"ok","timestamp":1768503470824,"user_tz":-180,"elapsed":12,"user":{"displayName":"Hakan Aytuğ Fırat","userId":"02958237446288372335"}}},"execution_count":26,"outputs":[]},{"cell_type":"code","source":["EPOCHS = 10\n","BATCH = 64\n","LR = 2e-3\n","\n","text_accs, text_f1s = [], []\n","\n","for fold_idx, (tr_idx, te_idx) in enumerate(folds, 1):\n","    print(f\"\\n===== TEXT BiLSTM | Fold {fold_idx} =====\")\n","\n","    tr_texts = df[\"text\"].iloc[tr_idx].tolist()\n","    te_texts = df[\"text\"].iloc[te_idx].tolist()\n","    ytr = df[\"y\"].iloc[tr_idx].values\n","    yte = df[\"y\"].iloc[te_idx].values\n","\n","    train_ds = TextDataset(tr_texts, ytr, word2idx, MAX_LEN)\n","    test_ds  = TextDataset(te_texts, yte, word2idx, MAX_LEN)\n","\n","    train_loader = DataLoader(train_ds, batch_size=BATCH, shuffle=True)\n","    test_loader  = DataLoader(test_ds, batch_size=BATCH, shuffle=False)\n","\n","    model = BiLSTM(len(word2idx)).to(device)\n","    opt = torch.optim.Adam(model.parameters(), lr=LR)\n","    loss_fn = nn.CrossEntropyLoss()\n","\n","    best_f1 = -1\n","    patience = 0\n","\n","    for ep in range(1, EPOCHS + 1):\n","        model.train()\n","        for xb, yb in train_loader:\n","            xb, yb = xb.to(device), yb.to(device)\n","            opt.zero_grad()\n","            loss = loss_fn(model(xb), yb)\n","            loss.backward()\n","            opt.step()\n","\n","        model.eval()\n","        preds, trues = [], []\n","        with torch.no_grad():\n","            for xb, yb in test_loader:\n","                xb = xb.to(device)\n","                p = torch.argmax(model(xb), dim=1).cpu().numpy()\n","                preds.extend(p)\n","                trues.extend(yb.numpy())\n","\n","        acc, f1m = compute_metrics(trues, preds)\n","\n","        print(f\"[TEXT_BiLSTM] ep {ep:02d}/{EPOCHS} | acc={acc:.3f} | macroF1={f1m:.3f}\")\n","\n","        if f1m > best_f1:\n","            best_f1 = f1m\n","            best_state = {k:v.detach().cpu().clone() for k,v in model.state_dict().items()}\n","            patience = 0\n","        else:\n","            patience += 1\n","        if patience >= 3:\n","            break\n","\n","    model.load_state_dict(best_state)\n","\n","    model.eval()\n","    preds, trues = [], []\n","    with torch.no_grad():\n","        for xb, yb in test_loader:\n","            xb = xb.to(device)\n","            preds.extend(torch.argmax(model(xb), 1).cpu().numpy())\n","            trues.extend(yb.numpy())\n","\n","    acc, f1m = compute_metrics(trues, preds)\n","    text_accs.append(acc)\n","    text_f1s.append(f1m)\n","\n","    print(f\"[TEXT_BiLSTM] Fold {fold_idx} FINAL | Acc={acc:.3f} | MacroF1={f1m:.3f}\")\n","    print(confusion_matrix(trues, preds))\n","\n","print(\"\\n=== TEXT_BiLSTM SUMMARY ===\")\n","print(\"Acc : %.3f ± %.3f\" % (np.mean(text_accs), np.std(text_accs)))\n","print(\"F1m : %.3f ± %.3f\" % (np.mean(text_f1s), np.std(text_f1s)))\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"7jeKzZiPBXuB","executionInfo":{"status":"ok","timestamp":1768503487661,"user_tz":-180,"elapsed":11132,"user":{"displayName":"Hakan Aytuğ Fırat","userId":"02958237446288372335"}},"outputId":"b5e455ca-9af7-46e9-a4a5-d8c34e203f7e"},"execution_count":27,"outputs":[{"output_type":"stream","name":"stdout","text":["\n","===== TEXT BiLSTM | Fold 1 =====\n","[TEXT_BiLSTM] ep 01/10 | acc=0.403 | macroF1=0.341\n","[TEXT_BiLSTM] ep 02/10 | acc=0.396 | macroF1=0.336\n","[TEXT_BiLSTM] ep 03/10 | acc=0.356 | macroF1=0.329\n","[TEXT_BiLSTM] ep 04/10 | acc=0.376 | macroF1=0.344\n","[TEXT_BiLSTM] ep 05/10 | acc=0.356 | macroF1=0.350\n","[TEXT_BiLSTM] ep 06/10 | acc=0.386 | macroF1=0.314\n","[TEXT_BiLSTM] ep 07/10 | acc=0.391 | macroF1=0.335\n","[TEXT_BiLSTM] ep 08/10 | acc=0.393 | macroF1=0.348\n","[TEXT_BiLSTM] Fold 1 FINAL | Acc=0.356 | MacroF1=0.350\n","[[63 55 32]\n"," [28 48 24]\n"," [65 55 32]]\n","\n","===== TEXT BiLSTM | Fold 2 =====\n","[TEXT_BiLSTM] ep 01/1