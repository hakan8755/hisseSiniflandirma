r k,v in enc.items()}\n","        out = bert(**enc).last_hidden_state  # (B, L, H)\n","        cls = out[:, 0, :]                  # (B, H)  -> DistilBERT CLS\n","        all_emb.append(cls.detach().cpu().numpy())\n","    return np.concatenate(all_emb, axis=0)\n","\n","# cache: aynı notebookta tekrar kullanmak için\n","if \"X_text_emb\" not in globals():\n","    X_text_emb = get_distilbert_cls_embeddings(df_w[\"text\"].values)\n","    print(\"X_text_emb:\", X_text_emb.shape)\n","else:\n","    print(\"✅ using cached X_text_emb:\", X_text_emb.shape)\n","\n","assert X_text_emb.shape[0] == Xw.shape[0] == len(yw)\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":66,"referenced_widgets":["acbde2593cc4461c8cf359ce7caf2d2e","0c6228d4d7d44e48a62854ced84b9e3d","5b73232344594d8ea7164fab699ff6b5","a1d9477ade844bd586bd137575d82a32","c7794115f6984af28bc32dfe9cf29e78","21fa256d00164e5d97e2f1fb8db2ec3c","da160c2bc91d4a0b99f7899d3a755a24","f8182e310a874fdab7466565cad6f9db","1518443e229d41c2929eeef72f023651","db98b75a63c346e5801f2d43b231bf16","75d757e169c34b948cc21f1596c5d793"]},"id":"SVh2VrB_LRus","executionInfo":{"status":"ok","timestamp":1768506096997,"user_tz":-180,"elapsed":2828,"user":{"displayName":"Hakan Aytuğ Fırat","userId":"02958237446288372335"}},"outputId":"2f2a3e23-8227-4e00-d2df-0064b13b1f6d"},"execution_count":61,"outputs":[{"output_type":"display_data","data":{"text/plain":["BERT CLS embed:   0%|          | 0/32 [00:00<?, ?it/s]"],"application/vnd.jupyter.widget-view+json":{"version_major":2,"version_minor":0,"model_id":"acbde2593cc4461c8cf359ce7caf2d2e"}},"metadata":{}},{"output_type":"stream","name":"stdout","text":["X_text_emb: (1991, 768)\n"]}]},{"cell_type":"code","source":["# Time flatten\n","X_time_flat = Xw.reshape(Xw.shape[0], -1)  # (N, 20*17)\n","print(\"X_time_flat:\", X_time_flat.shape)\n","\n","# Concat\n","X_early = np.concatenate([X_time_flat, X_text_emb], axis=1)\n","print(\"X_early:\", X_early.shape)\n","\n","y_early = yw.astype(np.int64)\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"S1PCl-cRLYqT","executionInfo":{"status":"ok","timestamp":1768506102560,"user_tz":-180,"elapsed":20,"user":{"displayName":"Hakan Aytuğ Fırat","userId":"02958237446288372335"}},"outputId":"41c42e63-65b7-4ec1-d908-d633dc319e4d"},"execution_count":62,"outputs":[{"output_type":"stream","name":"stdout","text":["X_time_flat: (1991, 340)\n","X_early: (1991, 1108)\n"]}]},{"cell_type":"code","source":["from sklearn.preprocessing import StandardScaler\n","from sklearn.metrics import accuracy_score, f1_score, confusion_matrix\n","\n","class EarlyMLP(nn.Module):\n","    def __init__(self, in_dim, hidden=512, drop=0.3, n_classes=3):\n","        super().__init__()\n","        self.net = nn.Sequential(\n","            nn.Linear(in_dim, hidden),\n","            nn.ReLU(),\n","            nn.Dropout(drop),\n","            nn.Linear(hidden, hidden//2),\n","            nn.ReLU(),\n","            nn.Dropout(drop),\n","            nn.Linear(hidden//2, n_classes),\n","        )\n","    def forward(self, x):\n","        return self.net(x)\n","\n","def train_early_mlp_cv(X, y, folds, epochs=30, batch=256, lr=1e-3, wd=1e-4, patience=5):\n","    accs, f1s = [], []\n","    all_cms = []\n","\n","    for fold_idx, (tr, te) in enumerate(folds, 1):\n","        Xtr, Xte = X[tr], X[te]\n","        ytr, yte = y[tr], y[te]\n","\n","        # scale\n","        scaler = StandardScaler()\n","        Xtr = scaler.fit_transform(Xtr)\n","        Xte = scaler.transform(Xte)\n","\n","        # torch tensors\n","        Xtr_t = torch.tensor(Xtr, dtype=torch.float32)\n","        ytr_t = torch.tensor(ytr, dtype=torch.long)\n","        Xte_t = torch.tensor(Xte, dtype=torch.float32)\n","        yte_t = torch.tensor(yte, dtype=torch.long)\n","\n","        train_loader = DataLoader(list(zip(Xtr_t, ytr_t)), batch_size=batch, shuffle=True)\n","        test_loader  = DataLoader(list(zip(Xte_t, yte_t)), batch_size=batch, shuffle=False)\n","\n","        model = EarlyMLP(in_dim=X.shape[1]).to(device)\n","        opt = torch.optim.AdamW(model.parameters(), lr=lr, weight_decay=wd)\n","        loss_fn = nn.CrossEntropyLoss()\n","\n","        best_f1, bad = -1, 0\n","        best_state = None\n","\n","        for ep in range(1, epochs+1):\n","            model.train()\n","            for xb, yb in train_loader:\n","                xb = xb.to(device); yb = yb.to(device)\n","                opt.zero_grad(set_to_none=True)\n","                logits = model(xb)\n","                loss = loss_fn(logits, yb)\n","                loss.backward()\n","                opt.step()\n","\n","            # eval\n","            model.eval()\n","            preds, trues = [], []\n","            with torch.no_grad():\n","                for xb, yb in test_loader:\n","                    xb = xb.to(device)\n","                    p = torch.argmax(model(xb), dim=1).cpu().numpy()\n","                    preds.extend(p.tolist())\n","                    trues.extend(yb.numpy().tolist())\n","\n","            acc = accuracy_score(trues, preds)\n","            f1m = f1_score(trues, preds, average=\"macro\")\n","\n","            if ep == 1 or ep % 2 == 0:\n","                print(f\"[EARLY_MLP] Fold {fold_idx} | ep {ep:02d}/{epochs} | acc={acc:.3f} | macroF1={f1m:.3f}\")\n","\n","            if f1m > best_f1 + 1e-4:\n","                best_f1 = f1m\n","                bad = 0\n","                best_state = {k:v.detach().cpu().clone() for k,v in model.state_dict().items()}\n","            else:\n","                bad += 1\n","                if bad >= patience:\n","                    break\n","\n","        model.load_state_dict(best_state)\n","        model.eval()\n","        preds, trues = [], []\n","        with torch.no_grad():\n","            for xb, yb in test_loader:\n","                xb = xb.to(device)\n","                p = torch.argmax(model(xb), dim=1).cpu().numpy()\n","                preds.extend(p.tolist())\n","                trues.extend(yb.numpy().tolist())\n","\n","        acc = accuracy_score(trues, preds)\n","        f1m = f1_score(trues, preds, average=\"macro\")\n","        cm = confusion_matrix(trues, preds)\n","\n","        accs.append(acc); f1s.append(f1m); all_cms.append(cm)\n","\n","        print(f\"\\n[EARLY_MLP] Fold {fold_idx} FINAL | Acc={acc:.3f} | MacroF1={f1m:.3f}\")\n","        print(cm)\n","\n","    print(\"\\n=== EARLY_MLP SUMMARY ===\")\n","    print(\"Acc : %.3f ± %.3f\" % (np.mean(accs), np.std(accs)))\n","    print(\"F1m : %.3f ± %.3f\" % (np.mean(f1s), np.std(f1s)))\n","\n","    return accs, f1s, all_cms\n","\n","early_accs, early_f1s, early_cms = train_early_mlp_cv(X_early, y_early, folds_ts)\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"6o7_K2kWLSWY","executionInfo":{"status":"ok","timestamp":1768506111399,"user_tz":-180,"elapsed":2397,"user":{"displayName":"Hakan Aytuğ Fırat","userId":"02958237446288372335"}},"outputId":"8e7a62fb-b66a-4c37-fd53-4bfc89ddbd15"},"execution_count":63,"outputs":[{"output_type":"stream","name":"stdout","text":["[EARLY_MLP] Fold 1 | ep 01/30 | acc=0.393 | macroF1=0.315\n","[EARLY_MLP] Fold 1 | ep 02/30 | acc=0.383 | macroF1=0.250\n","[EARLY_MLP] Fold 1 | ep 04/30 | acc=0.356 | macroF1=0.343\n","[EARLY_MLP] Fold 1 | ep 06/30 | acc=0.388 | macroF1=0.344\n","[EARLY_MLP] Fold 1 | ep 08/30 | acc=0.366 | macroF1=0.354\n","[EARLY_MLP] Fold 1 | ep 10/30 | acc=0.376 | macroF1=0.370\n","[EARLY_MLP] Fold 1 | ep 12/30 | acc=0.376 | macroF1=0.364\n","[EARLY_MLP] Fold 1 | ep 14/30 | acc=0.368 | macroF1=0.363\n","\n","[EARLY_MLP] Fold 1 FINAL | Acc=0.376 | MacroF1=0.370\n","[[53 32 54]\n"," [37 34 34]\n"," [48 44 63]]\n","[EARLY_MLP] Fold 2 | ep 01/30 | acc=0.367 | macroF1=0.310\n","[EARLY_MLP] Fold 2 | ep 02/30 | acc=0.367 | macroF1=0.281\n","[EARLY_MLP] Fold 2 | ep 04/30 | acc=0.375 | macroF1=0.340\n","[EARLY_MLP] Fold 2 | ep 06/30 | acc=0.385 | macroF1=0.362\n","[EARLY_MLP] Fold 2 | ep 08/30 | acc=0.372 | macroF1=0.350\n","[EARLY_MLP] Fold 2 | ep 10/30 | acc=0.367 | macroF1=0.345\n","\n","[EARLY_MLP] Fold 2 FINAL | Acc=0.385 | MacroF1=0.362\n","[[61 23 55]\n"," [34 20 50]\n"," [54 27 71]]\n","[EARLY_MLP] Fold 3 | ep 01/30 | acc=0.331 | macroF1=0.299\n","[EARLY_MLP] Fold 3 | ep 02/30 | acc=0.393 | macroF1=0.281\n","[EARLY_MLP] Fold 3 | ep 04/30 | acc=0.366 | macroF1=0.309\n","[EARLY_MLP] Fold 3 | ep 06/30 | acc=0.388 | macroF1=0.369\n","[EARLY_MLP] Fold 3 | ep 08/30 | acc=0.338 | macroF1=0.335\n","[EARLY_MLP] Fold 3 | ep 10/30 | acc=0.371 | macroF1=0.353\n","\n","[EARLY_MLP] Fold 3 FINAL | Acc=0.388 | MacroF1=0.369\n","[[59 17 62]\n"," [37 22 46]\n"," [67 15 74]]\n","[EARLY_MLP] Fold 4 | ep 01/30 | acc=0.369 | macroF1=0.291\n","[EARLY_MLP] Fold 4 | ep 02/30 | acc=0.382 | macroF1=0.325\n","[EARLY_MLP] Fold 4 | ep 04/30 | acc=0.364 | macroF1=0.359\n","[EARLY_MLP] Fold 4 | ep 06/30 | acc=0.367 | macroF1=0.334\n","[EARLY_MLP] Fold 4 | ep 08/30 | acc=0.399 | macroF1=0.377\n","[EARLY_MLP] Fold 4 | ep 10/30 | acc=0.392 | macroF1=0.320\n","[EARLY_MLP] Fold 4 | ep 12/30 | acc=0.374 | macroF1=0.350\n","\n","[EARLY_MLP] Fold 4 FINAL | Acc=0.399 | MacroF1=0.377\n","[[77 22 40]\n"," [50 22 32]\n"," [71 24 60]]\n","[EARLY_MLP] Fold 5 | ep 01/30 | acc=0.360 | macroF1=0.273\n","[EARLY_MLP] Fold 5 | ep 02/30 | acc=0.378 | macroF1=0.321\n","[EARLY_MLP] Fold 5 | ep 04/30 | acc=0.390 | macroF1=0.338\n","[EARLY_MLP] Fold 5 | ep 06/30 | acc=0.365 | macroF1=0.346\n","[EARLY_MLP] Fold 5 | ep 08/30 | acc=0.328 | macroF1=0.325\n","[EARLY_MLP] Fold 5 | ep 10/30 | acc=0.367 | macroF1=0.340\n","\n","[EARLY_MLP] Fold 5 FINAL | Acc=0.365 | MacroF1=0.346\n","[[46 19 74]\n"," [30 23 52]\n"," [53 26 77]]\n","\n","=== EARLY_MLP SUMMARY ===\n","Acc : 0.383 ± 0.012\n","F1m : 0.365 ± 0.011\n"]}]},{"cell_type":"code","source":["import numpy as np\n","import torch\n","import torch.nn as nn\n","from torch.utils.data import Dataset, DataLoader\n","from sklearn.preprocessing import StandardScaler\n","from sklearn.metrics import accuracy_score, f1_score, confusion_matrix\n","\n","device = \"cuda\" if torch.cuda.is_available() else \"cpu\"\n","print(\"device:\", device, \"| gpu:\", torch.cuda.get_device_name(0) if device==\"cuda\" else None)\n","\n","# -------------------------\n","# Dataset\n","# -------------------------\n","class SeqDataset(Dataset):\n","    def __init__(self, Xw, y):\n","        self.X = torch.from_numpy(Xw.astype(\"float32\"))  # (N,T,F)\n","        self.y = torch.from_numpy(y.astype(\"int64\"))\n","    def __len__(self):\n","        return len(self.y)\n","    def __getitem__(self, i):\n","        return self.X[i], self.y[i]\n","\n","# -------------------------\n","# Model (embedding support)\n","# -------------------------\n","class LSTMClassifier(nn.Module):\n","    def __init__(self, n_feat, hidden=128, n_layers=2, dropout=0.2, n_classes=3):\n","        super().__init__()\n","        self.hidden = hidden\n","        self.lstm = nn.LSTM(\n","            input_size=n_feat,\n","            hidden_size=hidden,\n","            num_layers=n_layers,\n","            batch_first=True,\n","            dropout=dropout if n_layers > 1 else 0.0,\n","            bidirectional=False\n","        )\n","        self.drop = nn.Dropout(dropout)\n","        self.fc = nn.Linear(hidden, n_classes)\n","\n","    def forward(self, x, return_emb=False):\n","        _, (hn, _) = self.lstm(x)      # hn: (n_layers, B, hidden)\n","        emb = hn[-1]                   # (B, hidden)\n","        logits = self.fc(self.drop(emb))\n","        if return_emb:\n","            return logits, emb\n","        return logits\n","\n","def metrics(y_true, y_pred):\n","    return accuracy_score(y_true, y_pred), f1_score(y_true, y_pred, average=\"macro\")\n","\n","# -------------------------\n","# Hyperparams\n","# -------------------------\n","EPOCHS   = 30\n","BATCH    = 256\n","LR       = 1e-3\n","WD       = 1e-4\n","PATIENCE = 5\n","MIN_DELTA = 1e-4  # best_f1 improvement threshold\n","\n","# -------------------------\n","# Outputs for fusion\n","# -------------------------\n","ts_proba_folds = []   # list of np arrays (N_test, 3)\n","ts_emb_folds   = []   # list of np arrays (N_test, hidden)\n","y_ts_folds     = []   # list of np arrays (N_test,)\n","idx_ts_folds   = []   # list of np arrays (N_test,) -> te_i\n","\n","# Summary metrics\n","lstm_accs, lstm_f1s = [], []\n","\n","# -------------------------\n","# Train/Eval per fold\n","# folds_ts must exist: list of (train_idx, test_idx)\n","# Xw: (N, T, F) | yw: (N,)\n","# -------------------------\n","assert \"folds_ts\" in globals(), \"folds_ts yok. Önce folds_ts oluştur.\"\n","assert \"Xw\" in globals() and \"yw\" in globals(), \"Xw/yw yok. Önce window datasını oluştur.\"\n","\n","for fold_idx, (tr_i, te_i) in enumerate(folds_ts, 1):\n","    Xtr, Xte = Xw[tr_i].copy(), Xw[te_i].copy()\n","    ytr, yte = yw[tr_i].copy(), yw[te_i].copy()\n","\n","    # --- scale (fit only train, apply to test) ---\n","    scaler = StandardScaler()\n","    Xtr2 = Xtr.reshape(-1, Xtr.shape[-1])\n","    Xte2 = Xte.reshape(-1, Xte.shape[-1])\n","    Xtr2 = scaler.fit_transform(Xtr2).astype(\"float32\")\n","    Xte2 = scaler.transform(Xte2).astype(\"float32\")\n","    Xtr = Xtr2.reshape(Xtr.shape).astype(\"float32\")\n","    Xte = Xte2.reshape(Xte.shape).astype(\"float32\")\n","\n","    train_loader = DataLoader(SeqDataset(Xtr, ytr), batch_size=BATCH, shuffle=True, drop_last=False)\n","    test_loader  = DataLoader(SeqDataset(Xte, yte), batch_size=BATCH, shuffle=False, drop_last=False)\n","\n","    model = LSTMClassifier(n_feat=Xtr.shape[-1]).to(device)\n","    opt = torch.optim.AdamW(model.parameters(), lr=LR, weight_decay=WD)\n","    loss_fn = nn.CrossEntropyLoss()\n","\n","    best_f1, patience = -1.0, 0\n","    best_state = None\n","\n","    for ep in range(1, EPOCHS + 1):\n","        # ---- train ----\n","        model.train()\n","        total_loss = 0.0\n","        for xb, yb in train_loader:\n","            xb, yb = xb.to(device), yb.to(device)\n","            opt.zero_grad(set_to_none=True)\n","            logits = model(xb)\n","            loss = loss_fn(logits, yb)\n","            loss.backward()\n","            opt.step()\n","            total_loss += loss.item() * len(yb)\n","\n","        # ---- eval (on test fold) ----\n","        model.eval()\n","        preds, trues = [], []\n","        with torch.no_grad():\n","            for xb, yb in test_loader:\n","                xb = xb.to(device)\n","                logits = model(xb)\n","                p = torch.argmax(logits, dim=1).cpu().numpy()\n","                preds.extend(p.tolist())\n","                trues.extend(yb.numpy().tolist())\n","\n","        acc, f1m = metrics(trues, preds)\n","\n","        if ep == 1 or ep % 2 == 0:\n","            print(f\"[TS_LSTM] Fold {fold_idx} | ep {ep:02d}/{EPOCHS} | \"\n","                  f\"loss={total_loss/len(tr_i):.4f} | acc={acc:.3f} | macroF1={f1m:.3f}\")\n","\n","        # early stopping on macroF1\n","        if f1m > best_f1 + MIN_DELTA:\n","            best_f1 = f1m\n","            patience = 0\n","            best_state = {k: v.detach().cpu().clone() for k, v in model.state_dict().items()}\n","        else:\n","            patience += 1\n","            if patience >= PATIENCE:\n","                break\n","\n","    # load best\n","    if best_state is not None:\n","        model.load_state_dict(best_state)\n","\n","    # ---- final collect: preds + proba + embedding ----\n","    model.eval()\n","    preds, trues = [], []\n","    probs_all, embs_all = [], []\n","\n","    with torch.no_grad():\n","        for xb, yb in test_loader:\n","            xb = xb.to(device)\n","            logits, emb = model(xb, return_emb=True)\n","            prob = torch.softmax(logits, dim=1)\n","\n","            p = torch.argmax(prob, dim=1).cpu().numpy()\n","            preds.extend(p.tolist())\n","            trues.extend(yb.numpy().tolist())\n","\n","            probs_all.append(prob.cpu().numpy())\n","            embs_all.append(emb.cpu().numpy())\n","\n","    probs_all = np.concatenate(probs_all, axis=0)  # (N_test, 3)\n","    embs_all  = np.concatenate(embs_all, axis=0)   # (N_test, hidden)\n","    trues_np  = np.array(trues, dtype=np.int64)\n","\n","    acc, f1m = metrics(trues_np, np.array(preds))\n","    lstm_accs.append(acc); lstm_f1s.append(f1m)\n","\n","    # store for fusion alignment\n","    ts_proba_folds.append(probs_all)\n","    ts_emb_folds.append(embs_all)\n","    y_ts_folds.append(trues_np)\n","    idx_ts_folds.append(te_i.copy())\n","\n","    print(f\"\\n[TS_LSTM] Fold {fold_idx} FINAL | Acc={acc:.3f} | MacroF1={f1m:.3f}\")\n","    print(confusion_matrix(trues_np, np.array(preds)))\n","\n","print(\"\\n=== TS_LSTM SUMMARY ===\")\n","print(\"Acc : %.3f ± %.3f\" % (np.mean(lstm_accs), np.std(lstm_accs)))\n","print(\"F1m : %.3f ± %.3f\" % (np.mean(lstm_f1s), np.std(lstm_f1s)))\n","\n","# quick sanity\n","print(\"\\nSaved folds:\")\n","print(\"ts_proba_folds:\", len(ts_proba_folds), \"| shape[0]:\", ts_proba_folds[0].shape if len(ts_proba_folds)>0 else None)\n","print(\"ts_emb_folds  :\", len(ts_emb_folds),   \"| shape[0]:\", ts_emb_folds[0].shape if len(ts_emb_folds)>0 else None)\n","print(\"y_ts_folds    :\", len(y_ts_folds),     \"| len[0]:\",   len(y_ts_folds[0]) if len(y_ts_folds)>0 else None)\n","print(\"idx_ts_folds  :\", len(idx_ts_folds),   \"| len[0]:\",   len(idx_ts_folds[0]) if len(idx_ts_folds)>0 else None)\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"L9KS54eRMjr9","executionInfo":{"status":"ok","timestamp":1768506414150,"user_tz":-180,"elapsed":3715,"user":{"displayName":"Hakan Aytuğ Fırat","userId":"02958237446288372335"}},"outputId":"307a8c1c-d924-49e2-e214-4af771a395d5"},"execution_count":64,"outputs":[{"output_type":"stream","name":"stdout","text":["device: cuda | gpu: NVIDIA A100-SXM4-80GB\n","[TS_LSTM] Fold 1 | ep 01/30 | loss=1.0930 | acc=0.386 | macroF1=0.254\n","[TS_LSTM] Fold 1 | ep 