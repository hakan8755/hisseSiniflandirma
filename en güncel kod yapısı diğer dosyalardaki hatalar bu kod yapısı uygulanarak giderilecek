,"daily_close[\"ret_next\"] = daily_close[\"close\"].pct_change().shift(-1)\n","\n","THR = 0.14\n","def make_label_thr(r, thr):\n","    if pd.isna(r): return None\n","    if r > thr: return \"UP\"\n","    if r < -thr: return \"DOWN\"\n","    return \"FLAT\"\n","\n","daily_close[\"label\"] = daily_close[\"ret_next\"].apply(lambda r: make_label_thr(r, THR))\n","daily_close = daily_close.dropna(subset=[\"label\"]).reset_index(drop=True)\n","\n","print(\"AMZN daily shape:\", daily_close.shape)\n","print(\"AMZN label dist:\\n\", daily_close[\"label\"].value_counts())\n","\n","# 4) tabular daily (close hariç feature'lar)\n","exclude = {\"datetime\",\"day\",\"ticker\",\"ticker_name\",\"label\",\"ret_next\",\"close\"}\n","feat_cols_amzn = [c for c in prices_f.columns if c not in exclude]\n","\n","tab_daily = (prices_f.sort_values(\"datetime\")\n","             .groupby(\"day\", as_index=False)\n","             .agg({c:\"last\" for c in feat_cols_amzn}))\n","\n","data_amzn = daily_close.merge(tab_daily, on=\"day\", how=\"left\").sort_values(\"day\").reset_index(drop=True)\n","data_amzn[feat_cols_amzn] = data_amzn[feat_cols_amzn].ffill()\n","data_amzn = data_amzn.dropna(subset=feat_cols_amzn + [\"label\"]).reset_index(drop=True)\n","\n","print(\"data_amzn shape:\", data_amzn.shape)\n","print(\"label dist after NaN clean:\\n\", data_amzn[\"label\"].value_counts())\n","\n","# 5) comments: 2018+ (zaten AMZN yorumları) + day aggregation\n","comments_f = comments.copy()\n","comments_f[\"datetime\"] = pd.to_datetime(comments_f[\"datetime\"], errors=\"coerce\")\n","comments_f = comments_f[comments_f[\"datetime\"] >= START].copy()\n","comments_f[\"day\"] = comments_f[\"datetime\"].dt.floor(\"D\")\n","comments_f[\"comment\"] = comments_f[\"comment\"].astype(str)\n","comments_f = comments_f[comments_f[\"comment\"].str.strip().ne(\"\")].copy()\n","\n","text_daily = (comments_f.groupby(\"day\", as_index=False)\n","              .agg(text=(\"comment\", lambda x: \" \".join(x.tolist())),\n","                   n_comments=(\"comment\",\"size\")))\n","text_daily[\"text_len\"] = text_daily[\"text\"].str.len()\n","\n","# 6) final df_amzn\n","df_amzn = data_amzn.merge(text_daily, on=\"day\", how=\"left\")\n","df_amzn[\"text\"] = df_amzn[\"text\"].fillna(\"\")\n","df_amzn[\"n_comments\"] = df_amzn[\"n_comments\"].fillna(0).astype(int)\n","df_amzn[\"text_len\"] = df_amzn[\"text_len\"].fillna(0).astype(int)\n","\n","label_map = {\"DOWN\":0, \"FLAT\":1, \"UP\":2}\n","df_amzn[\"y\"] = df_amzn[\"label\"].map(label_map).astype(int)\n","\n","print(\"\\nFINAL df_amzn shape:\", df_amzn.shape)\n","print(\"label dist:\\n\", df_amzn[\"label\"].value_counts())\n","print(\"text empty ratio:\", (df_amzn[\"text_len\"]==0).mean())\n","\n","# 7) guideline check\n","counts = df_amzn[\"label\"].value_counts()\n","print(\"\\nGuideline check min class:\", int(counts.min()), \"| total:\", int(counts.sum()))\n","assert counts.sum() >= 1500 and counts.min() >= 500, \"Guideline class constraint bozuldu. THR yeniden ayarlanmalı.\"\n","print(\"✅ Guideline constraint OK on AMZN-only data\")\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":627},"id":"WK4tsiVHCchN","executionInfo":{"status":"error","timestamp":1768503760305,"user_tz":-180,"elapsed":168,"user":{"displayName":"Hakan Aytuğ Fırat","userId":"02958237446288372335"}},"outputId":"3fa5f083-109e-4cb8-9702-a3d46c401851"},"execution_count":32,"outputs":[{"output_type":"stream","name":"stdout","text":["prices_f tickers: ['AMZN']\n","prices_f shape: (2011, 21) | min/max: 2018-01-02 00:00:00 -> 2025-12-31 00:00:00\n","AMZN daily shape: (2010, 4)\n","AMZN label dist:\n"," label\n","FLAT    2009\n","DOWN       1\n","Name: count, dtype: int64\n","data_amzn shape: (2010, 21)\n","label dist after NaN clean:\n"," label\n","FLAT    2009\n","DOWN       1\n","Name: count, dtype: int64\n","\n","FINAL df_amzn shape: (2010, 25)\n","label dist:\n"," label\n","FLAT    2009\n","DOWN       1\n","Name: count, dtype: int64\n","tex